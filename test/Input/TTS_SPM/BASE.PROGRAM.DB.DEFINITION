	PERMANENT STRING S.RESULT.FIX.SCORE := "" ; ! SCORE
	PERMANENT STRING S.RESULT.FIX.NBEST := "" ; ! N-BEST

! SYSTEMCALL PARAMETER(Common.SystemCallPre(), Common.SystemCallPost())
	PERMANENT NUMBER N.CID := 0 ;

! CONTEXT PARAMETER
	! C&C("ACCURACY"=1500,"HYPOTHESES.RESULT"=8,"HYPOTHESES.SEARCH"=8,"TS.FX"=1000)
	PERMANENT NUMBER N.ACCURACY.VALUE := 1500 ;
	PERMANENT NUMBER N.HYPOTHESES.RESULT.VALUE := 8 ;
	PERMANENT NUMBER N.HYPOTHESES.SEARCH.VALUE := 8 ;
	PERMANENT NUMBER N.TS.FX.VALUE := 1000 ;
	
	! SDCARD("ACCURACY"=10000,"HYPOTHESES.RESULT"=10,"HYPOTHESES.SEARCH"=20,"TS.FX"=1000)
	PERMANENT NUMBER N.ACCURACY.SDCARD.VALUE := 10000 ;
	PERMANENT NUMBER N.HYPOTHESES.RESULT.SDCARD.VALUE := 10 ;
	PERMANENT NUMBER N.HYPOTHESES.SEARCH.SDCARD.VALUE := 20 ;
	PERMANENT NUMBER N.TS.FX.SDCARD.VALUE := 1000 ;

! CONTROL
	$N.CONTROL.ERROR := @N_FLAG_OFF ;
	$N.CONTROL.RELAY := @N_FLAG_OFF ;
	
! LEX FORMAT
	$N.RULE.FORMAT := 1 ; ! LEX DB 검색 방식 설정 1 : String, 2 : ID
	$N.RESULT.FORMAT := 1 ; ! LEX DB 구조 방식 설정 1 : string, 2 : ID, 3 : ID+String, Default : String, VDE : ID

! FLOW
	$N.CONTROL.FLOW := @N_STAGE_DONE ;
	$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
	
! BEEP EXPRESSION
	$S.PROMPT.BEEP.EXPRESSION := "" ; ! LP 2.6에서 미사용 변수
	$S.PROMPT.BEEP.TEXT := "" ;
	$S.PROMPT.EXIT.EXPRESSION := "\@N_PROMPT_EXIT" ; ! DIALOG EXIT
	$S.PROMPT.EXIT.TEXT := "" ;
	$S.PROMPT.STOP.EXPRESSION := "\@N_PROMPT_SILENCE" ; ! DIALOG STOP
	$S.PROMPT.STOP.TEXT := "" ;

! TTS RESOURCE
	$N.OUTPUT.RESOURCE.STREAM := 1 ; ! initializer value : 0 , Talk stream은 Tima에서 확인 하므로 0(Talk Stream)이 아닌 값을 전달 해야 다시 load 함
	
! HYBRID SERVER SETTING
! DIRECT SERVER : [KOK] @N_SERVER_TYPE_KAKAO [ENU] @N_SERVER_TYPE_NUANCE [FRC,SPM,EU] @N_SERVER_TYPE_NONE
! RELAY SERVER : [KOK] @N_SERVER_TYPE_RELAY_SERVER
	N.SERVER.SETTING := @N_SERVER_TYPE_NONE ;
