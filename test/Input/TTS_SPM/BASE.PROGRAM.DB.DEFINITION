	PERMANENT STRING S.SLOT.SECOND.TAG   := "" ; ! SLOT2 TAG 
	PERMANENT STRING S.SLOT.SECOND.VALUE := "" ; ! SLOT2 VALUE
	PERMANENT STRING S.SLOT.SECOND.SCORE := "" ; ! SLOT2 SCORE
	
