JINIE Pre.Boot ()
	PERMANENT STRING S.PROGRAM.PREPROCESSOR := "Pre.Base" ;
	PERMANENT STRING S.PROGRAM.RECOGNIZER := "Common.Recognizer" ;
	PERMANENT STRING S.PROGRAM.POSTPROCESSOR := "Post.Exit" ;
	PERMANENT STRING S.PROGRAM.INTERRUPT := "Post.Interrupt" ;

	CALL Base.Variable () ;
	CALL System.Param.ASR () ;
	CALL System.Param.TTS () ;

	!$S.PROGRAM.PREPROCESSOR := S.PROGRAM.PREPROCESSOR ;
	!$S.PROGRAM.RECOGNIZER := S.PROGRAM.RECOGNIZER ;
	!$S.INTERRUPT.PROCESSOR := S.PROGRAM.INTERRUPT ;
	!$S.PROGRAM.POSTPROCESSOR := S.PROGRAM.POSTPROCESSOR ;
EOP

! Quick Mode / MAINMENU
JINIE Pre.Base ()
	
	$S.OUTPUT.RESOURCE.EXPRESSION := "\"DICTIONARY\"(\"/storage/vr/LPTE/DATA/ENU/UserDic_TTS_enu.dcb\") + \"DB\"(\"/storage/vr/LPTE/DATA/ENU/apdb_rp_ava_enuvr.dat\")" ;
	LOG "LPLOG: RESULT : $S.OUTPUT.RESOURCE.EXPRESSION = " + $S.OUTPUT.RESOURCE.EXPRESSION ;
EOP

JINIE Pre.VirtualTtsBt ()

	$S.OUTPUT.RESOURCE.EXPRESSION := "\"DICTIONARY\"(\"/storage/vr/LPTE/DATA/ENU/UserDic_TTS_enu.dcb\") + \"DB\"(\"/storage/vr/LPTE/DATA/ENU/apdb_rp_ava_enu.dat\") + \"DB\"(\"/storage/vr/LPTE/DATA/ENU/apdb_rp_ava_enuvr.dat\")" ;
	LOG "LPLOG: RESULT : $S.OUTPUT.RESOURCE.EXPRESSION = " + $S.OUTPUT.RESOURCE.EXPRESSION ;
