JINIE Base.Variable ()

! VAD
	PERMANENT NUMBER N.STORE.NBEST := 0 ; ! N-BEST COUNT SAVED
	PERMANENT NUMBER N.INNER.CONFIDENCE.FIRST.VALUE := 0 ;
	PERMANENT NUMBER N.INNER.CONFIDENCE.SECOND.VALUE := 0 ;
	PERMANENT NUMBER N.INNER.CONFIDENCE.GAP.VALUE := 0 ; ! FIRST CONFIDENCE - SECOND CONFIDENCE
	
! FREQUENCY
	PERMANENT STRING S.SLOT.RESULT := "" ; ! FREQUENCY NUMBER RESULT
	PERMANENT STRING S.INNER.RADIOSTATION.INFO := "" ; ! 방송국명 리스트에서 TAB 변경 시 인식된 방송국명 정보를 전달 하기 위하여 저장
	
! POI NAME
	PERMANENT STRING S.NAVI.CURRENT.REGION := "" ; ! STATE/REGION STRING
	
! POI CATEGORY
	PERMANENT NUMBER N.GENRE.COUNT.BIG := 0 ;
	PERMANENT NUMBER N.GENRE.COUNT.MID := 0 ;
	PERMANENT NUMBER N.GENRE.COUNT.SMALL := 0 ;
	PERMANENT STRING S.INNER.CATEGORY.INFO := "" ; ! FIND POI CATEGORY에서 TAB 변경 시 인식된 CATEGORY 정보를 전달 하기 위하여 저장
	PERMANENT STRING S.GENRE.CODE := "" ;
	
! SCENARIO CONTROL
	PERMANENT NUMBER N.RESULT.ID := 0 ; ! SLOT ID(STRING -> NUMBER)
	PERMANENT STRING S.PRE.RESULT.FIX.CID := "" ; ! 이전 RULE TAG 저장
	PERMANENT STRING S.INNER.TEMP.STRING := "" ; ! PROGRAM SCENARIO 중 임시로 필요한 내용 저장
	PERMANENT NUMBER N.INNER.NAVIGATION.HELP.FLAG := 0 ; ! Find Address/Find Address <state>/Find the Nearest POI/POIs near destination 시나리오에서 quick/help navi category 확인 Quick : 0, Help navi category : 1 - back
	PERMANENT STRING S.INNER.SCREEN.STRING := "" ; ! 시나리오 중간에 상태 값이 변경 되었을 때 진입 시점 상태에 대하여 시나리오 천이 : YES/NO PROGRAM

! S.ACTION.STRING
	PERMANENT STRING S.ACTION.STRING := "" ; ! SLOT INFO

! PROMPT / SCREEN TEXT
	PERMANENT STRING S.INNER.PROMPT.KINDLY.STR := "" ;
	PERMANENT STRING S.INNER.PROMPT.SIMPLY.STR := "" ;
	PERMANENT STRING S.INNER.PROMPT.DISPLY.STR := "" ;

! TIMEOUT / REJECTION
	PERMANENT NUMBER N.INNER.RECOG.COUNT.NUM := 0 ; ! TIMEOUT COUNT + REJECTON COUNT
	PERMANENT NUMBER N.CONTROL.NOLEADINGSILENCE := 0 ; ! NO LEADING SILENCE COUNT

! DIALOG
	PERMANENT NUMBER N.DIALOG := 0 ; ! CURRENT DIALOG ID
	
! BEEP
	PERMANENT STRING S.BEEP := "" ; ! LISTEN BEEP
	
! GRAMMAR
	PERMANENT STRING S.GRAMMAR := "" ; ! GRAMMAR NAME
	PERMANENT NUMBER N.GRAMMAR := 0 ; ! GRAMMAR NUMBERS
	PERMANENT STRING S.ACTIVATION := "" ; ! ACTIVATE RULE
	PERMANENT STRING S.BNF.GRAMMAR := "" ;
	PERMANENT STRING S.BNF.ACTIVATION := "" ;
	
! SLM GRAMMAR
	PERMANENT STRING S.SLM.GRAMMAR := "" ;
	PERMANENT STRING S.SLM.GRAMMAR.BT.FULL := "" ;
	PERMANENT STRING S.SLM.GRAMMAR.BT.REVERSE := "" ;
	
! LOG
	PERMANENT STRING S.INNER.LOG.STRING := "" ;

! REFERENCE_KEY
	PERMANENT STRING S.DOMAIN.VALUE := "" ; ! DOMAIN
	PERMANENT STRING S.INTENT.VALUE := "" ; ! INTENTION

	PERMANENT NUMBER N.REF.DB := 0 ; ! REFERENCE DB
	PERMANENT NUMBER N.REF.ID := 0 ; ! REFERENCE DB SEARCH FIELD ID
	PERMANENT STRING S.REF.KEY := "" ; ! REFERENCE KEY

	PERMANENT NUMBER N.RESULT.CONUT.NUM :=  0 ; ! N-BEST COUNT
	PERMANENT NUMBER N.REF.NBEST.COUNT := 1 ; ! NBEST 결과 시 REFERENCE DB를 검색할 횟수
	
	PERMANENT STRING S.RESULT.CID := "" ; ! COMMAND ID
	PERMANENT STRING S.RESULT.CMD := "" ; ! RECOG COMMAND
	PERMANENT STRING S.RESULT.DIP := "" ; ! DISPLAY TEXT
	PERMANENT STRING S.RESULT.SCORE := "" ; ! SCORE
	
	PERMANENT STRING S.RESULT.DOMAIN := "" ; ! DOMAIN
	PERMANENT STRING S.RESULT.EXAMPLE := "" ; ! EXAMPLE
	PERMANENT STRING S.RESULT.INTENTION := "" ; ! INTENTION

	PERMANENT STRING S.SLOT.FIRST.ID := "" ; ! Slot1 ID
	PERMANENT STRING S.SLOT.FIRST.TAG   := "" ; ! SLOT1 TAG
	PERMANENT STRING S.SLOT.FIRST.VALUE := "" ; ! SLOT1 VALUE
	PERMANENT STRING S.SLOT.FIRST.SCORE := "" ; ! SLOT1 SCORE
	
	PERMANENT STRING S.SLOT.SECOND.ID := "" ; ! Slot2 ID
	PERMANENT STRING S.SLOT.SECOND.TAG   := "" ; ! SLOT2 TAG 
	PERMANENT STRING S.SLOT.SECOND.VALUE := "" ; ! SLOT2 VALUE
	PERMANENT STRING S.SLOT.SECOND.SCORE := "" ; ! SLOT2 SCORE
	
	PERMANENT STRING S.SLOT.THIRD.ID := "" ; ! Slot3 ID
	PERMANENT STRING S.SLOT.THIRD.TAG   := "" ; ! SLOT3 TAG
	PERMANENT STRING S.SLOT.THIRD.VALUE := "" ; ! SLOT3 VALUE
	PERMANENT STRING S.SLOT.THIRD.SCORE := "" ; ! SLOT3 SCORE
	
! HEADER RESULT : FIX 1-BEST
	PERMANENT STRING S.RESULT.FIX.CID := "" ; ! COMMAND ID
	PERMANENT STRING S.RESULT.FIX.CMD := "" ; ! RECOG COMMAND
	PERMANENT STRING S.RESULT.FIX.DIP := "" ; ! DISPLAY TEXT
	PERMANENT STRING S.RESULT.FIX.SCORE := "" ; ! SCORE
	PERMANENT STRING S.RESULT.FIX.NBEST := "" ; ! N-BEST

! SYSTEMCALL PARAMETER(Common.SystemCallPre(), Common.SystemCallPost())
	PERMANENT NUMBER N.CID := 0 ;

! CONTEXT PARAMETER
	! C&C("ACCURACY"=1500,"HYPOTHESES.RESULT"=8,"HYPOTHESES.SEARCH"=16,"TS.FX"=1000)
	PERMANENT NUMBER N.ACCURACY.VALUE := 1500 ;
	PERMANENT NUMBER N.HYPOTHESES.RESULT.VALUE := 8 ;
	PERMANENT NUMBER N.HYPOTHESES.SEARCH.VALUE := 16 ;
	PERMANENT NUMBER N.TS.FX.VALUE := 1000 ;
	
	! SDCARD("ACCURACY"=6700,"HYPOTHESES.RESULT"=8,"HYPOTHESES.SEARCH"=16,"TS.FX"=1000)
	PERMANENT NUMBER N.ACCURACY.SDCARD.VALUE := 6700 ;
	PERMANENT NUMBER N.HYPOTHESES.RESULT.SDCARD.VALUE := 8 ;
	PERMANENT NUMBER N.HYPOTHESES.SEARCH.SDCARD.VALUE := 16 ;
	PERMANENT NUMBER N.TS.FX.SDCARD.VALUE := 1000 ;

! CONTROL
	$N.CONTROL.ERROR := @N_FLAG_OFF ;
	$N.CONTROL.RELAY := @N_FLAG_OFF ;
	
! LEX FORMAT
	$N.RULE.FORMAT := 1 ; ! LEX DB 검색 방식 설정 1 : String, 2 : ID, Default : String, VDE, POI NAME : ID
	$N.RESULT.FORMAT := 1 ; ! LEX DB 구조 방식 설정 1 : string, 2 : ID, 3 : ID+String

! FLOW
	$N.CONTROL.FLOW := @N_STAGE_DONE ;
	$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
	
! BEEP EXPRESSION
	$S.PROMPT.BEEP.EXPRESSION := "" ; ! LP 2.6에서 미사용 변수
	$S.PROMPT.BEEP.TEXT := "" ;
	$S.PROMPT.EXIT.EXPRESSION := "\@N_PROMPT_EXIT" ; ! DIALOG EXIT
	$S.PROMPT.EXIT.TEXT := "" ;
	$S.PROMPT.STOP.EXPRESSION := "\@N_PROMPT_SILENCE" ; ! DIALOG STOP
	$S.PROMPT.STOP.TEXT := "" ;

! TTS RESOURCE
	$N.OUTPUT.RESOURCE.STREAM := 1 ; ! initializer value : 0 , Talk stream은 Tima에서 확인 하므로 0(Talk Stream)이 아닌 값을 전달 해야 다시 load 함
	
! HYBRID SERVER SETTING
! DIRECT SERVER : [KOK] @N_SERVER_TYPE_KAKAO [ENU] @N_SERVER_TYPE_NUANCE [FRC,SPM,PTB,EU] @N_SERVER_TYPE_NONE
! RELAY SERVER : [KOK,ENU] @N_SERVER_TYPE_RELAY_SERVER
	N.SERVER.SETTING := @N_SERVER_TYPE_RELAY_SERVER ;
EOP