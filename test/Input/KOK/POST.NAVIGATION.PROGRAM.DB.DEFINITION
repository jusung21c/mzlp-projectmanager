JINIE Post.NavigationMain ()

	! CHECK MULTI-MODAL INTERRUPT
	CALL Common.CheckInterrupt () ;
	
	IF ( $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 0 ) |
		( $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 4 ) THEN
		BEGIN
			! CONTROL PROCESS & PROGRAM
			CALL Common.ControlPost () ;
			
			! SET REFERENCE DB & REFERENCE KEY
			S.INTENT.VALUE := $S.STT.RESULT.INTENTION[1][1] ; ! INTENTION SAVED
			S.SLOT.FIRST.TAG := $S.STT.RESULT.SLOT[1][1] ; ! SLOT1 TAG SAVED
			S.SLOT.SECOND.TAG := $S.STT.RESULT.SLOT[1][4] ; ! SLOT2 TAG SAVED
			S.SLOT.THIRD.TAG := $S.STT.RESULT.SLOT[1][7] ; ! SLOT3 TAG SAVED
			
			N.REF.DB := @N_REFERENCE_INFO ;
			N.REF.ID := 11 ;
			S.REF.KEY := S.INTENT.VALUE + S.SLOT.FIRST.TAG + S.SLOT.SECOND.TAG + S.SLOT.THIRD.TAG ;
			
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GANGWONDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGGIDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGSANGNAMDO" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGSANGBUKDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GWANGJU" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_DAEGU" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_DAEJEON" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_BUSAN" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_SEOUL" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_SEJONG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_ULSAN" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_INCHEON" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEOLLANAMDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEOLLABUKDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEJUDO" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_CHUNGCHEONGNAMDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_CHUNGCHEONGBUKDO" THEN
				BEGIN
					N.REF.DB := @N_REFERENCE_CATEGORY ;
					N.REF.ID := 1 ;
					S.REF.KEY := "\@N_TAG_POI_NAME_ID" ;
				END
			ELSE
				BEGIN
					IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_BIG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_MID" THEN
						BEGIN ! ONLY MULTI-MODAL
							N.REF.DB := @N_REFERENCE_CATEGORY ;
							N.REF.ID := 1 ;
							S.REF.KEY := $S.RESULT.ID[1] ;
						END
					ELSE
						BEGIN
							IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_FIND_POI" & $S.STT.RESULT.SLOT[1][1] = "<SLOT_CATEGORY_NAME>" THEN
								BEGIN
									N.REF.DB := @N_REFERENCE_CATEGORY ;
									N.REF.ID := 2 ;
									S.REF.KEY := $S.STT.RESULT.SLOT[1][2] ;
								END
							ELSE
								BEGIN
									! SERVER INFO 전달
									IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_NAVIGATION_SERVER" & $S.STT.RESULT.SLOT[1][1] = "SearchPhrase" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_WAIT_SERVER" THEN
										BEGIN
											CALL Common.ExtraInfo () ;
										END
								END
						END
				END
				
			CALL Common.ReferenceKey (N.REF.DB, N.REF.ID, S.REF.KEY) ;
		END
		
	IF ( $N.STATUS.PROCESS = @N_PROCESS_RESULT & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 0 ) |
		( $N.STATUS.PROCESS = @N_PROCESS_RESULT & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 4 ) THEN
		BEGIN
			! ART
			CALL Common.ARTIntentionSetting () ;
			
			! GET N-best Result From Reference DB
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GANGWONDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGGIDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGSANGNAMDO" | 
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GYEONGSANGBUKDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_GWANGJU" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_DAEGU" | 
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_DAEJEON" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_BUSAN" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_SEOUL" | 
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_SEJONG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_ULSAN" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_INCHEON" | 
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEOLLANAMDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEOLLABUKDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_JEJUDO" |
				$S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_CHUNGCHEONGNAMDO" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_POI_NAME_CHUNGCHEONGBUKDO" THEN
				BEGIN
					CALL Common.ReferenceNBestResultPoiName () ;
				END
			ELSE
				BEGIN
					! GET 1-Best Result From Reference DB
					IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_BIG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_MID" THEN
						BEGIN ! ONLY MULTI-MODAL
							CALL Common.ReferenceOneBestResultPoiCategory () ;
							S.INNER.CATEGORY.INFO := S.ACTION.STRING ; ! 인식된 카테고리 정보 저장
						END
					ELSE
						BEGIN
							IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_FIND_POI" & $S.STT.RESULT.SLOT[1][1] = "<SLOT_CATEGORY_NAME>" THEN
								BEGIN
									CALL Common.ReferenceOneBestResultPoiCategory () ;
									S.INNER.CATEGORY.INFO := S.ACTION.STRING ; ! 인식된 카테고리 정보 저장
								END
							ELSE
								BEGIN
									CALL Common.ReferenceOneBestResult () ;
									
									IF $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_NEAR_CURRENT_POSITION" | $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_NEAR_DESTINATION" | $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_ALONG_ROUTE" THEN
										BEGIN
											S.ACTION.STRING := S.INNER.CATEGORY.INFO ; ! 인식된 카테고리 정보 전달
										END
								END
						END
				END
				
			! SET ACTION-VALUE
			CALL Common.SetActionValue () ;
			
			! SET RULE-TAG
			CALL Common.SetPreRuleTag () ;
			
			! SYSTEM CALL
			N.CID ~= S.RESULT.FIX.CID ;
			CALL Common.SystemCallPost (N.CID) ;
			
			! CALL LP PROCEDURE
			IF S.RESULT.FIX.CID = "\@N_TAG_SHOW_MAP_ID" THEN
				BEGIN
					CALL LpProc.ShowMap () ;
				END

			IF S.RESULT.FIX.CID = "\@N_TAG_DESTINATION_SEARCH_ID" THEN
				BEGIN
					CALL LpProc.DestinationSearch () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_POI_NAME_ID" THEN
				BEGIN
					CALL LpProc.PoiName () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NAVI_GANGWONDO_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_GYEONGGIDO_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_GYEONGSANGNAMDO_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_NAVI_GYEONGSANGBUKDO_ID" | S.RESULT.FIX.CID= "\@N_TAG_NAVI_GWANGJU_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_DAEGU_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_NAVI_DAEJEON_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_BUSAN_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_SEOUL_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_NAVI_SEJONG_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_ULSAN_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_INCHEON_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_NAVI_JEOLLANAMDO_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_JEOLLABUKDO_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_JEJUDO_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_NAVI_CHUNGCHEONGNAMDO_ID" | S.RESULT.FIX.CID = "\@N_TAG_NAVI_CHUNGCHEONGBUKDO_ID" THEN
				BEGIN
					CALL LpProc.NaviLocation () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ID" THEN
				BEGIN
					CALL LpProc.FindPoi () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_CATEGORY_ID" THEN
				BEGIN
					CALL LpProc.FindPoiCategory () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_CATEGORY_NAME_ID" THEN
				BEGIN
					CALL LpProc.CategoryName () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NEAR_CURRENT_POSITION_ID" THEN
				BEGIN
					CALL LpProc.NearCurrentPosition () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NEAR_DESTINATION_ID" THEN
				BEGIN
					CALL LpProc.NearDestination () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ALONG_ROUTE_ID" THEN
				BEGIN
					CALL LpProc.AlongRoute () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_POINT_ID" THEN
				BEGIN
					CALL LpProc.PreviousPoint () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_DESTINATION_ID" THEN
				BEGIN
					CALL LpProc.PreviousDestination () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_SEARCH_ID" THEN
				BEGIN
					CALL LpProc.PreviousSearch () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_START_ID" THEN
				BEGIN
					CALL LpProc.PreviousStart () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TRAFFIC_INFO_ID" THEN
				BEGIN
					CALL LpProc.TrafficInfo () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_ON_ID" THEN
				BEGIN
					CALL LpProc.TurnGuidanceOn () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_OFF_ID" THEN
				BEGIN
					CALL LpProc.TurnGuidanceOff () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DESTINATION_INFO_ID" THEN
				BEGIN
					CALL LpProc.DestinationInfo () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ROUTE_OPTION_ID" THEN
				BEGIN
					CALL LpProc.RouteOption () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_SHOW_ROUTE_ID" THEN
				BEGIN
					CALL LpProc.ShowRoute () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ADD_WAYPOINT_ID" THEN
				BEGIN
					CALL LpProc.AddWaypoint () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_GO_HOME_ID" THEN
				BEGIN
					CALL LpProc.GoHome () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_GO_TO_OFFICE_ID" THEN
				BEGIN
					CALL LpProc.GoToOffice () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_STOP_GUIDANCE_ID" THEN
				BEGIN
					CALL LpProc.StopGuidance () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_RESUME_GUIDANCE_ID" THEN
				BEGIN
					CALL LpProc.ResumeGuidance () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_FASTER_TIME_ID" THEN
				BEGIN
					CALL LpProc.FindFasterTime () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FASTER_TIME_ID" THEN
				BEGIN
					CALL LpProc.FasterTime () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_SHORTER_DISTANCE_ID" THEN
				BEGIN
					CALL LpProc.FindShorterDistance () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_SHORTER_DISTANCE_ID" THEN
				BEGIN
					CALL LpProc.ShorterDistance () ;
				END
				
			! SERVER
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_SERVER_ID" THEN
				BEGIN
					CALL LpProc.FindServer () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ADDRESS_SERVER_ID" THEN
				BEGIN
					CALL LpProc.FindPoiAddressServer () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_POI_ADDRESS_SERVER_ID" THEN
				BEGIN
					CALL LpProc.PoiAddressServer () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_WAIT_SERVER_ID" THEN
				BEGIN
					CALL Common.ServerVRExit () ;
				END
				
			! REPEAT CONTROL
			CALL Common.RepeatControl () ;
			
			! FEEDBACK PROMPT CONTROL
			CALL Common.FeedbackPromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.FeedbackOnScreenText () ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_TRANSITION & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			! VR 종료 후, MAINMENU Dialog 진입 시 정상 Prompt 출력을 위하여 초기화
			IF $N.TRANSITION.DIALOG = @N_DIALOG_STOP | $N.TRANSITION.DIALOG = @N_DIALOG_EXIT THEN
				BEGIN
					S.RESULT.FIX.CID := "" ;
				END
		END
EOP

! "지도 보기" 명령어 처리 프로시저
JINIE LpProc.ShowMap ()

	IF N.SYS.MIRRORLINK.DEVICE.CONNECT = 200000 | N.SYS.MIRRORLINK.NAVI.CONDITION = 200000 | N.NAVI.SDCARD.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.SYS.MIRRORLINK.DEVICE.CONNECT = 1 & N.SYS.MIRRORLINK.NAVI.CONDITION = 1 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_REPEAT ;
				END
			ELSE
				BEGIN
					IF N.NAVI.SDCARD.EXIST = 0 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ;
							S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK ;
						END
					ELSE
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_11_01" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_K3_11_01" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							N.EXIT.ACTION.TRANSITION := 19101 ;
							$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
							$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
						END
				END
		END
EOP

! "목적지 설정" (Embedded Spec), "길 안내" (Server Spec) 명령어 처리 프로시저
JINIE LpProc.DestinationSearch ()

	IF N.NAVI.CURRENT.REGION = 200000 | N.SYS.MIRRORLINK.DEVICE.CONNECT = 200000 | N.SYS.MIRRORLINK.NAVI.CONDITION = 200000 | N.NAVI.SDCARD.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.SYS.MIRRORLINK.DEVICE.CONNECT = 1 & N.SYS.MIRRORLINK.NAVI.CONDITION = 1 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_REPEAT ;
				END
			ELSE
				BEGIN
					IF N.NAVI.SDCARD.EXIST = 0 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ;
							S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK ;
						END
					ELSE
						BEGIN
							IF N.VENDER.VR.MODE = 1 THEN
								BEGIN
									IF N.NETWORK.SUBSCRIBED = 1 THEN
										BEGIN
											IF N.NETWORK.SETTING = 1 THEN
												BEGIN
													! 중계 서버 신호 세기가 약할 때, Embedded에서 Gate Command가 전달된 경우 예외 처리(Revision 2060)
													IF $S.STT.RESULT.OFFLINE.INTENTION[1][1] = "WaitServerPOI" THEN
														BEGIN
															CALL Common.ServerVRExit () ;
														END
													ELSE
														BEGIN
															S.PRE.RESULT.FIX.CID := "\@N_TAG_FIND_SERVER_ID" ;
															S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_09" ;
															S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_09" ;
															S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_1_0_52" + "\n" + "\@S_SCREEN_PROMPT_LID_3_1_0_53" ;
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_SERVER ;
														END
												END
											ELSE
												BEGIN
													CALL LpProc.RegionStringSetting () ;
													S.INNER.PROMPT.KINDLY.STR := S.NAVI.CURRENT.REGION + " " + "\@S_DIALOG_PROMPT_LID_K3_1_1_01" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_01" ;
													S.INNER.PROMPT.DISPLY.STR := "" ;
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_POI_LOCATION ;
												END
										END
									ELSE
										BEGIN
											IF N.SERVER.EXPIRED.PROMPT.CHECK = 1 THEN
												BEGIN
													CALL LpProc.RegionStringSetting () ;
													IF N.VENDER.NETWORK = 1 THEN
														BEGIN
															S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_13" ;
															S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_13" ;
														END
													ELSE
														BEGIN
															S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_14" ;
															S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_14" ;
														END
													S.INNER.PROMPT.DISPLY.STR := "" ;
													$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_POI_LOCATION ;
												END
											ELSE
												BEGIN
													CALL LpProc.RegionStringSetting () ;
													S.INNER.PROMPT.KINDLY.STR := S.NAVI.CURRENT.REGION + " " + "\@S_DIALOG_PROMPT_LID_K3_1_1_01" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_01" ;
													S.INNER.PROMPT.DISPLY.STR := "" ;
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_POI_LOCATION ;
												END
										END
								END
							ELSE
								BEGIN
									CALL LpProc.RegionStringSetting () ;
									S.INNER.PROMPT.KINDLY.STR := S.NAVI.CURRENT.REGION + " " + "\@S_DIALOG_PROMPT_LID_K3_1_1_01" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_01" ;
									S.INNER.PROMPT.DISPLY.STR := "" ;
									$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_POI_LOCATION ;
								END
						END
				END
		END
EOP

! "<시 or 도 이름>" 명령어 처리 프로시저
