! 인식된 시설명의 ID로 GENRE CODE TABLE에서 GENRE CODE를 검색하는 프로시저
JINIE LpProc.CategoryGenreCode ()

	TEMPORARY NUMBER N.TEMP := 2 ;
	
	S.GENRE.CODE := "" ;
	N.RESULT.ID ~= S.SLOT.FIRST.ID ;
	
	! GENRE CODE
	IF N.RESULT.ID > 606100 & N.RESULT.ID < 606200 THEN ! BIG
		BEGIN
			WHILE N.TEMP < N.GENRE.COUNT.BIG DO
				BEGIN
					IF S.GENRE.TABLE.BIG[N.TEMP][4] = S.SLOT.FIRST.ID THEN
						BEGIN
							S.GENRE.CODE := S.GENRE.TABLE.BIG[N.TEMP][3] ;
						END
					N.TEMP := N.TEMP + 1 ;
				END
		END
	ELSE
		BEGIN
			IF N.RESULT.ID > 606200 & N.RESULT.ID < 606300 THEN ! MID
				BEGIN
					WHILE N.TEMP < N.GENRE.COUNT.MID DO
						BEGIN
							IF S.GENRE.TABLE.MID[N.TEMP][4] = S.SLOT.FIRST.ID THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.TABLE.MID[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1 ;
						END
				END
			ELSE ! SMALL
				BEGIN
					WHILE N.TEMP < N.GENRE.COUNT.SMALL DO
						BEGIN
							IF S.GENRE.TABLE.SMALL[N.TEMP][4] = S.SLOT.FIRST.ID THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.TABLE.SMALL[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1 ;
						END
				END
		END

	LOG "LP LOG : LpProc.CategoryGenreCode - S.GENRE.CODE = " + S.GENRE.CODE ;
EOP

