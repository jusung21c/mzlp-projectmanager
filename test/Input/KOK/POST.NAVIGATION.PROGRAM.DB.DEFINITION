JINIE LpProc.PoiAddressServer ()
	
	S.INNER.TEMP.STRING := "" ;
	
	IF N.NAVI.SEARCH.RESULT.NUM = 200000 | N.NAVI.DESTINATION.SETTING = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.NAVI.SEARCH.RESULT.NUM = 0 THEN
				BEGIN
					S.INNER.TEMP.STRING := "SEARCH_RESULT_NUM_ZERO" ;
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_12" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_12" ;
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_1_0_52" + "\n" + "\@S_SCREEN_PROMPT_LID_3_1_0_53" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_SERVER ;
				END
			ELSE
				BEGIN
					IF N.NAVI.SEARCH.RESULT.NUM = 1 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_04" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_04" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							
							IF N.NAVI.DESTINATION.SETTING = 0 THEN
								BEGIN
									IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
										BEGIN
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_SELECT ;
										END
									ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
										BEGIN
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_SELECT ;
										END
								END
							ELSE ! N.NAVI.DESTINATION.SETTING = 1
								BEGIN
									IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
										BEGIN
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_WAY_SELECT ;
										END
									ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
										BEGIN
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_WAY_SELECT ;
										END
								END
						END
					ELSE ! N.NAVI.SEARCH.RESULT.NUM > 1
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_03" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_03" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_LIST ;
						END
				END
		END
EOP

! 서버의 "길 안내 <시설명>" or "길 안내 <주소>" 명령어 처리 프로시저
JINIE LpProc.FindPoiAddressServer ()

	IF N.NAVI.SEARCH.RESULT.NUM = 200000 | N.NAVI.DESTINATION.SETTING = 200000 | N.SYS.MIRRORLINK.DEVICE.CONNECT = 200000 | N.SYS.MIRRORLINK.NAVI.CONDITION = 200000 | N.NAVI.SDCARD.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.SYS.MIRRORLINK.DEVICE.CONNECT = 1 & N.SYS.MIRRORLINK.NAVI.CONDITION = 1 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_REPEAT ;
				END
			ELSE
				BEGIN
					IF N.NAVI.SDCARD.EXIST = 0 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ;
							S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK ;
						END
					ELSE
						BEGIN
							IF N.NAVI.SEARCH.RESULT.NUM = 0 THEN
								BEGIN
									S.INNER.TEMP.STRING := "SEARCH_RESULT_NUM_ZERO" ;
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_12" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_12" ;
									S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_1_0_52" + "\n" + "\@S_SCREEN_PROMPT_LID_3_1_0_53" ;
									$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_SERVER ;
								END
							ELSE
								BEGIN
									IF N.NAVI.SEARCH.RESULT.NUM = 1 THEN
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_04" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_04" ;
											S.INNER.PROMPT.DISPLY.STR := "" ;
											
											IF N.NAVI.DESTINATION.SETTING = 0 THEN
												BEGIN
													IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_SELECT ;
														END
													ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_SELECT ;
														END
												END
											ELSE ! N.NAVI.DESTINATION.SETTING = 1
												BEGIN
													IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_WAY_SELECT ;
														END
													ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_WAY_SELECT ;
														END
												END
										END
									ELSE ! N.NAVI.SEARCH.RESULT.NUM > 1
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_03" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_03" ;
											S.INNER.PROMPT.DISPLY.STR := "" ;
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_LIST ;
										END
								END
						END
				END
		END
EOP
