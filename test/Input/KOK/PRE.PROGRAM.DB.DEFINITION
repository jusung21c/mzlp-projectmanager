JINIE Pre.Boot ()
	PERMANENT STRING S.PROGRAM.PREPROCESSOR := "Pre.Base" ;
	PERMANENT STRING S.PROGRAM.RECOGNIZER := "Common.Recognizer" ;
	PERMANENT STRING S.PROGRAM.POSTPROCESSOR := "Post.Exit" ;
	PERMANENT STRING S.PROGRAM.INTERRUPT := "Post.Interrupt" ;
	
	CALL Base.Variable () ;
	CALL System.Param.ASR () ;
	CALL System.Param.TTS () ;
	
	!$S.PROGRAM.PREPROCESSOR := S.PROGRAM.PREPROCESSOR ;
	$S.PROGRAM.RECOGNIZER := S.PROGRAM.RECOGNIZER ;
	$S.INTERRUPT.PROCESSOR := S.PROGRAM.INTERRUPT ;
	!$S.PROGRAM.POSTPROCESSOR := S.PROGRAM.POSTPROCESSOR ;
EOP

! Quick Mode / MAINMENU
JINIE Pre.Base ()
	
	IF $N.STATUS.PROCESS = @N_PROCESS_INIT THEN
		BEGIN
			! CURRENT DIALOG SETTING
			N.DIALOG := $N.STATUS.DIALOG ;
			S.INNER.LOG.STRING ~= N.DIALOG ;
			LOG "LP LOG : Pre.Base - N.DIALOG = " + S.INNER.LOG.STRING ;
			
			! INITIALIZER
			CALL Common.Initializer () ;
			
			! CONTROL PROCESS & PROGRAM
			CALL Common.ControlPre (N.DIALOG) ;
			
			! MULTI MODAL KEYWORD
			CALL Common.ContextKeywords(N.DIALOG) ;
			
			! TIMEOUT PARAMETER
			CALL System.Param.Timeout (N.DIALOG) ;
			
			! BARGE IN SETTING
			CALL System.BargeinSettingControl (N.DIALOG) ;
			
			! SYSTEMCALL
			CALL Common.SystemCallPre (N.DIALOG) ;
			
			! SERVER SETTING
			CALL Common.ServerSettingControl (N.DIALOG) ;
			
			! LEX DB LOAD
			CALL Common.Lex (N.DIALOG) ;
			
			! SD-CARD CHECK
			IF N.NAVI.SDCARD.EXIST = 1 THEN
				BEGIN
					! GENRE TABLE SETTING
					N.GENRE.COUNT.BIG ~= S.GENRE.TABLE.BIG[1][1] ;
					N.GENRE.COUNT.MID ~= S.GENRE.TABLE.MID[1][1] ;
					N.GENRE.COUNT.SMALL ~= S.GENRE.TABLE.SMALL[1][1] ;
			
					N.GENRE.COUNT.BIG := N.GENRE.COUNT.BIG + 2 ;
					N.GENRE.COUNT.MID := N.GENRE.COUNT.MID + 2 ;
					N.GENRE.COUNT.SMALL := N.GENRE.COUNT.SMALL + 2 ;
				END
			ELSE
				BEGIN
					N.GENRE.COUNT.BIG := 0 ;
					N.GENRE.COUNT.MID := 0 ;
					N.GENRE.COUNT.SMALL := 0 ;
				END
			
			! GRAMMAR LOAD LOAD
			CALL Common.GrammarLoad (N.DIALOG) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_GUIDANCE THEN
		BEGIN
			! PROMPT
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_6_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_6_01" ;
			S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_2_01" ;
			
			! FULL BARGE IN PROMPT 처리 : REPEAT DIALOG
			IF S.RESULT.FIX.CID = "\@N_TAG_PRESET_NUMBER_ID" | S.RESULT.FIX.CID = "\@N_TAG_PLAY_ID" | S.RESULT.FIX.CID = "\@N_TAG_PAUSE_ID" | S.RESULT.FIX.CID = "\@N_TAG_PLAY_TRACK_NUMBER_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_SHUFFLE_ID" | S.RESULT.FIX.CID = "\@N_TAG_SHUFFLE_OFF_ID" | S.RESULT.FIX.CID = "\@N_TAG_REPEAT_ID" | S.RESULT.FIX.CID = "\@N_TAG_REPEAT_OFF_ID" |
				S.RESULT.FIX.CID = "\@N_TAG_STATION_LIST_ID" | S.RESULT.FIX.CID = "\@N_TAG_RADIO_AST_ID" | S.RESULT.FIX.CID = "\@N_TAG_DMB_AST_ID" THEN
				BEGIN
					IF N.AV.CONDITION = 0 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_04" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_04" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
						END
					ELSE
						BEGIN
							IF N.GLOBAL.CONDITION = 0 THEN
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
									S.INNER.PROMPT.DISPLY.STR := "" ;
								END
							ELSE
								BEGIN
									IF S.RESULT.FIX.CID = "\@N_TAG_STATION_LIST_ID" | S.RESULT.FIX.CID = "\@N_TAG_RADIO_AST_ID" THEN
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
											S.INNER.PROMPT.DISPLY.STR := "" ;
										END
								END
						END
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DESTINATION_SEARCH_ID" | S.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ID" | S.RESULT.FIX.CID = "\@N_TAG_FIND_CATEGORY_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_SHOW_MAP_ID" | S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_POINT_ID" | S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_DESTINATION_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_SEARCH_ID" | S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_START_ID" | S.RESULT.FIX.CID = "\@N_TAG_TRAFFIC_INFO_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_GO_HOME_ID" | S.RESULT.FIX.CID = "\@N_TAG_GO_TO_OFFICE_ID" | S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_ON_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_OFF_ID" | S.RESULT.FIX.CID = "\@N_TAG_DESTINATION_INFO_ID" | S.RESULT.FIX.CID = "\@N_TAG_ROUTE_OPTION_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_FIND_FASTER_TIME_ID" | S.RESULT.FIX.CID = "\@N_TAG_FIND_SHORTER_DISTANCE_ID" | S.RESULT.FIX.CID = "\@N_TAG_SHOW_ROUTE_ID" | 
				S.RESULT.FIX.CID = "\@N_TAG_STOP_GUIDANCE_ID" | S.RESULT.FIX.CID = "\@N_TAG_RESUME_GUIDANCE_ID" THEN
				BEGIN
					IF N.SYS.MIRRORLINK.DEVICE.CONNECT = 1 & N.SYS.MIRRORLINK.NAVI.CONDITION = 1 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_03" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_03" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
						END
				END
				
			! GUIDANCE PROMPT CONTROL
			CALL Common.GuidancePromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.GuidanceOnScreenText () ;
		END
EOP

