! Embedded에서 3rd Timeout or Rejection 발생 시 처리 프로시저
! EMBEDDED RESULT TYPE : TIMEOUT, REJECTION
JINIE LpProc.InnerResult ()

	IF $N.RESULT.TYPE = @N_RESULT_TYPE_TIMEOUT THEN
		BEGIN
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_10_02" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_10_02" ;
			S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_10_03" ;
			$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
		END
		
	IF $N.RESULT.TYPE = @N_RESULT_TYPE_REJECTION | $N.RESULT.TYPE = @N_RESULT_TYPE_RESULT THEN
		BEGIN ! TYPE_RESULT 의 경우, Post.Garbage를 통하여 Rejection 3회 발생했을 때를 위하여 확인
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_10_05" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_10_05" ;
			S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_10_06" ;
			$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
		END
EOP

