! 현재 Dialog에서 SCENARIO DB에 정의되지 않은 RULE이 전달되는 경우, Rejection 처리 프로시저
JINIE Post.Garbage ()

	IF $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			CALL Common.EmbeddedRejection () ;
		END
EOP

JINIE Post.Interrupt ()

	! UNINITIAL
	N.EXIT.ACTION.TRANSITION := 99999 ;
	$S.RESULT.STRING := "" ;
	$S.STT.RESULT.STRING := "" ;
	$S.STT.RESULT.ONLINE.STRING := "" ;
	$S.STT.RESULT.OFFLINE.STRING := "" ;
	$N.INTERRUPT.TYPE := 0 ;
	N.VOLUME.SILENCE := 0 ;
	S.RESULT.FIX.CID := "0" ;
	
	S.INNER.LOG.STRING ~= $N.INTERRUPT.FORMAT ;
	LOG "LP LOG : Post.Interrupt - $N.INTERRUPT.FORMAT = " + S.INNER.LOG.STRING ;
	
	! BEEP
	IF $N.INTERRUPT.FORMAT = 0 THEN
		BEGIN
			! Beep on
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
		END
	ELSE
		BEGIN
			! Beep off
			$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
		END
EOP
