							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_CATEGORY ;
