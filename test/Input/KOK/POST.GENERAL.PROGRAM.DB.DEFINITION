			S.INNER.PROMPT.KINDLY.STR := "" ;
			S.INNER.PROMPT.SIMPLY.STR := "" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
			
			! PHONE_CALL, BT_AUDIO
			IF S.INNER.SCREEN.STRING = "\@S_SCREEN_DEVICE_CONNECT_SELECT" THEN
