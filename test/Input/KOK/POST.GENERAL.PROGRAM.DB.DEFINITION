! "예" 명령어 처리 프로시저(Navi)
JINIE LpProc.NaviYes ()

	IF N.NAVI.HOME.REGISTERED = 200000 | N.NAVI.OFFICE.REGISTERED = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			S.INNER.PROMPT.KINDLY.STR := "" ;
			S.INNER.PROMPT.SIMPLY.STR := "" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
			
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_POI_NAME_ID" THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_04" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_04" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					N.EXIT.ACTION.TRANSITION := 19011 ;
					$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
					$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
				END
			
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_POINT_ID" | S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_DESTINATION_ID" |
			S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_SEARCH_ID" | S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_START_ID" THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_04" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_04" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
					$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
					
					IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_POINT_ID" THEN
						BEGIN
							N.EXIT.ACTION.TRANSITION := 19111 ;
						END
					IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_DESTINATION_ID" THEN
						BEGIN
							N.EXIT.ACTION.TRANSITION := 19112 ;
						END
					IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_SEARCH_ID" THEN
						BEGIN
							N.EXIT.ACTION.TRANSITION := 19113 ;
						END
					IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_START_ID" THEN
						BEGIN
							N.EXIT.ACTION.TRANSITION := 19114 ;
						END
				END
				
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_CATEGORY_ID" | S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ID" THEN
				BEGIN
					IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_YESNO_SELECT THEN
