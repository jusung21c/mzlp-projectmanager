JINIE Post.GeneralMain ()

	! CHECK MULTI-MODAL INTERRUPT
	CALL Common.CheckInterrupt () ;
	
	IF ( $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 0 ) |
		( $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 4 ) THEN
		BEGIN
			! CONTROL PROCESS & PROGRAM
			CALL Common.ControlPost () ;
			
			! SET REFERENCE DB & REFERENCE KEY
			S.INTENT.VALUE := $S.STT.RESULT.INTENTION[1][1] ; ! INTENTION SAVED
			S.SLOT.FIRST.TAG := $S.STT.RESULT.SLOT[1][1] ; ! SLOT1 TAG SAVED
			S.SLOT.SECOND.TAG := $S.STT.RESULT.SLOT[1][4] ; ! SLOT2 TAG SAVED
			S.SLOT.THIRD.TAG := $S.STT.RESULT.SLOT[1][7] ; ! SLOT3 TAG SAVED
			
			N.REF.DB := @N_REFERENCE_INFO ;
			N.REF.ID := 11 ;
			S.REF.KEY := S.INTENT.VALUE + S.SLOT.FIRST.TAG + S.SLOT.SECOND.TAG + S.SLOT.THIRD.TAG ;
			
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_INNER_RESULT" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_SERVER_INNER_RESULT" THEN
				BEGIN
					N.REF.DB := @N_REFERENCE_INFO ;
					N.REF.ID := 1 ;
					S.REF.KEY := $S.RESULT.ID[1][1] ;
				END
				
			CALL Common.ReferenceKey (N.REF.DB, N.REF.ID, S.REF.KEY) ;
		END
		
	IF ( $N.STATUS.PROCESS = @N_PROCESS_RESULT & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 0 ) |
		( $N.STATUS.PROCESS = @N_PROCESS_RESULT & $N.STATUS.PROGRAM = @N_FLAG_ON & $N.INTERRUPT.TYPE = 4 ) THEN
		BEGIN
			! ART
			CALL Common.ARTIntentionSetting () ;
			
			! GET 1-Best Result From Reference DB
			CALL Common.ReferenceOneBestResult () ;
			
			! SET ACTION-VALUE
			CALL Common.SetActionValue () ;
			
			! SET RULE-TAG
			CALL Common.SetPreRuleTag () ;
			
			! SYSTEM CALL
			N.CID ~= S.RESULT.FIX.CID ;
			CALL Common.SystemCallPost (N.CID) ;
			
			! CALL LP PROCEDURE
			IF S.RESULT.FIX.CID = "\@N_TAG_EXIT_ID" THEN
				BEGIN
					CALL LpProc.Exit () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_BACK_ID" THEN
				BEGIN
					CALL LpProc.Back () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_YES_ID" THEN
				BEGIN
					CALL LpProc.Yes () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NAVI_YES_ID" THEN
				BEGIN
					CALL LpProc.NaviYes () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NO_ID" THEN
				BEGIN
					CALL LpProc.No () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NAVI_NO_ID" THEN
				BEGIN
					CALL LpProc.NaviNo () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_LINE_NUMBER_ID" THEN
				BEGIN
					CALL LpProc.LineNumber () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NAVI_LINE_NUMBER_ID" THEN
				BEGIN
					CALL LpProc.NaviLineNumber () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NEXT_PAGE_ID" THEN
				BEGIN
					CALL LpProc.NextPage () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_PAGE_ID" THEN
				BEGIN
					CALL LpProc.PreviousPage () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_MANUAL_PTT_ID" THEN
				BEGIN
					CALL LpProc.ManualPTT () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_MANUAL_NETWORK_DISCONNECTED_ID" THEN
				BEGIN
					CALL LpProc.ManualNetworkDisconnected () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_INNER_RESULT_ID" THEN
				BEGIN
					CALL LpProc.InnerResult () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_SERVER_INNER_RESULT_ID" THEN
				BEGIN
					CALL LpProc.ServerInnerResult () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DRIVING_BACK_ID" | S.RESULT.FIX.CID = "\@N_TAG_EXIT_BACK_ID" THEN
				BEGIN
					CALL LpProc.InnerBack () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_EXIT_EXIT_ID" THEN
				BEGIN
					CALL LpProc.ExitExit () ;
				END
				
			! REPEAT CONTROL
			CALL Common.RepeatControl () ;
			
			! FEEDBACK PROMPT CONTROL
			CALL Common.FeedbackPromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.FeedbackOnScreenText () ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_TRANSITION & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			! VR 종료 후, MAINMENU Dialog 진입 시 정상 Prompt 출력을 위하여 초기화
			IF $N.TRANSITION.DIALOG = @N_DIALOG_STOP | $N.TRANSITION.DIALOG = @N_DIALOG_EXIT THEN
				BEGIN
					S.RESULT.FIX.CID := "" ;
				END
		END
EOP

! "종료" 명령어 처리 프로시저
JINIE LpProc.Exit ()

	IF $N.INTERRUPT.TYPE = 4 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_9_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_9_01" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			N.EXIT.ACTION.TRANSITION := 99999 ;
			$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
		END
EOP

! "이전 화면" 명령어 처리 프로시저
JINIE LpProc.Back ()

	S.INNER.PROMPT.KINDLY.STR := S.INNER.PROMPT.KINDLY.STR ;
	S.INNER.PROMPT.SIMPLY.STR := S.INNER.PROMPT.SIMPLY.STR ;
	S.INNER.PROMPT.DISPLY.STR := S.INNER.PROMPT.DISPLY.STR ;
	$N.TRANSITION.DIALOG := @N_DIALOG_BACK ;
	
	IF $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_CATEGORY THEN
		BEGIN
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_6_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_6_01" ;
			S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_2_01" ;
			$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU ;
		END
		
	! BACK DIALOG STACK EXCEPTION
	IF $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_PHONE | $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_NAVIGATION | $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_DMB |
		$N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_RADIO | $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_MEDIA THEN
		BEGIN
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_11_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_11_01" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU_HELP_CATEGORY ;
		END
		
	IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_CATEGORY | $N.STATUS.DIALOG = @N_DIALOG_NAVI_POI_LOCATION | $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_SERVER THEN
		BEGIN
			IF N.INNER.NAVIGATION.HELP.FLAG = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_6_01" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_6_01" ;
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_2_01" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU ;
				END
			ELSE
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_11_03" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_11_03" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU_HELP_NAVIGATION ;
				END
		END
		
	IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_NBEST_LIST THEN
		BEGIN
			$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_POI_LOCATION ;
		END
		
	IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_CATEGORY_LIST | $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_CATEGORY_NONE_LIST THEN ! stack 예외처리 : FIND_POI, FIND_CATEGORY
		BEGIN
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ID" THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_CATEGORY ;
				END
			
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_CATEGORY_ID" THEN ! ONE SHOT
				BEGIN
					IF N.INNER.NAVIGATION.HELP.FLAG = 0 THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_6_01" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_6_01" ;
							S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_2_01" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU ;
						END
					ELSE
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_11_03" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_11_03" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_MAINMENU_HELP_NAVIGATION ;
						END
				END
		END
EOP

! "예" 명령어 처리 프로시저(Phone, DMB)
