													S.INNER.PROMPT.KINDLY.STR := S.AVN.SELECT.STATION.SLOT[1][1] + " " + "\@S_DIALOG_PROMPT_LID_K4_4_1_04" ;
													S.INNER.PROMPT.SIMPLY.STR := S.AVN.SELECT.STATION.SLOT[1][1] ;
													S.INNER.PROMPT.DISPLY.STR := "" ;
													N.EXIT.ACTION.TRANSITION := 7023 ;
												END
											ELSE
												BEGIN
													S.INNER.PROMPT.KINDLY.STR := S.AVN.SELECT.STATION.SLOT[1][1] + " " + "\@S_DIALOG_PROMPT_LID_K4_6_0_02" ;
													S.INNER.PROMPT.SIMPLY.STR := S.AVN.SELECT.STATION.SLOT[1][1] ;
													S.INNER.PROMPT.DISPLY.STR := "" ;
													N.EXIT.ACTION.TRANSITION := 7023 ;
												END
										END
								END
							$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
							$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
						END
						
					IF $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_TRACKLIST THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := S.AVN.SELECT.SLOT + " " + "\@S_DIALOG_PROMPT_LID_K4_10_3_02" ;
							S.INNER.PROMPT.SIMPLY.STR := S.AVN.SELECT.SLOT + " " + "\@S_DIALOG_PROMPT_LID_S4_10_3_02" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							N.EXIT.ACTION.TRANSITION := 16009 ;
							$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
							$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
						END
				END
		END
EOP

! "목록 <number>" 명령어 처리 프로시저(Navi)
JINIE LpProc.NaviLineNumber ()

	IF N.GLOBAL.LISTITEM.NUM = 200000 | N.NAVI.SEARCH.RESULT.NUM = 200000 | N.NAVI.DESTINATION.SETTING = 200000 | N.NAVI.PHONE.NUMBER.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			N.RESULT.ID ~= S.SLOT.FIRST.ID ;
			
			S.INNER.PROMPT.KINDLY.STR := "" ;
			S.INNER.PROMPT.SIMPLY.STR := "" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
			
			IF N.RESULT.ID > N.GLOBAL.LISTITEM.NUM THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_14_02" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_14_02" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_REPEAT ;
				END
			ELSE
				BEGIN
					IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_NBEST_LIST THEN
						BEGIN
							IF N.NAVI.SEARCH.RESULT.NUM = 0 THEN ! Mobis Request Exception
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_SCREEN_PROMPT_LID_POI_NAME_ZERO" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_SCREEN_PROMPT_LID_POI_NAME_ZERO" ;
									S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_POI_NAME_ZERO" ;
									$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_EXIT ;
								END
							ELSE
								BEGIN
									IF N.NAVI.SEARCH.RESULT.NUM = 1 THEN
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_04" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_04" ;
											S.INNER.PROMPT.DISPLY.STR := "" ;
											
											IF N.NAVI.DESTINATION.SETTING = 0 THEN
												BEGIN
													IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_SELECT ;
														END
													ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_SELECT ;
														END
												END
											ELSE ! N.NAVI.DESTINATION.SETTING = 1
												BEGIN
													IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_WAY_SELECT ;
														END
													ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_WAY_SELECT ;
														END
												END
										END
									ELSE
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_03" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_03" ;
											S.INNER.PROMPT.DISPLY.STR := "" ;
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_LIST ;
										END
								END
						END
					ELSE
						BEGIN
							IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_LIST | $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_CATEGORY_LIST THEN
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_04" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_04" ;
									S.INNER.PROMPT.DISPLY.STR := "" ;
									
									IF N.NAVI.DESTINATION.SETTING = 0 THEN
										BEGIN
											IF S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ADDRESS_SERVER_ID" | S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_SERVER_ID" THEN
												BEGIN
													S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_1_04" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_1_04" ;
													S.INNER.PROMPT.DISPLY.STR := "" ;
													N.EXIT.ACTION.TRANSITION := 19011 ;
													$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
													$N.TRANSITION.DIALOG := @N_DIALOG_STOP ;
												END
											ELSE
												BEGIN
													IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_SELECT ;
														END
													ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
														BEGIN
															$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_SELECT ;
														END
												END
										END
									ELSE ! N.NAVI.DESTINATION.SETTING = 1
										BEGIN
											IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_WAY_SELECT ;
												END
											ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_WAY_SELECT ;
												END
										END
								END
							ELSE ! NAVI_PREVIOUS_POINT / NAVI_PREVIOUS_DESTINATION / NAVI_PREVIOUS_SEARCH / NAVI_PREVIOUS_START
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_4_04" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_4_04" ;
									S.INNER.PROMPT.DISPLY.STR := "" ;
									
									IF N.NAVI.DESTINATION.SETTING = 0 THEN
										BEGIN
											IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_PRE_YESNO_SELECT ;
												END
											ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_PRE_YESNO_CALL_SELECT ;
												END
										END
									ELSE ! N.NAVI.DESTINATION.SETTING = 1
										BEGIN
											IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_PRE_WAY_SELECT ;
												END
											ELSE !N.NAVI.PHONE.NUMBER.EXIST = 1
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_PRE_CALL_WAY_SELECT ;
												END
										END
								END
						END
				END
		END
EOP

