JINIE Pre.NoRecogBack ()

	IF $N.STATUS.PROCESS = @N_PROCESS_INIT THEN
		BEGIN
			! CURRENT DIALOG SETTING
			N.DIALOG := $N.STATUS.DIALOG ;
			S.INNER.LOG.STRING ~= N.DIALOG ;
			LOG "LP LOG : Pre.NoRecogBack - N.DIALOG = " + S.INNER.LOG.STRING ;
			
			! CONTROL
			CALL Common.ControlPre (N.DIALOG) ;
			
			! MULTI MODAL KEYWORD
			CALL Common.ContextKeywords(N.DIALOG) ;
			
			! TIMEOUT PARAMETER
			CALL System.Param.Timeout (N.DIALOG) ;
			
			! BARGE IN SETTING
			CALL System.BargeinSettingControl (N.DIALOG) ;
			
			! SYSTEMCALL
			CALL Common.SystemCallPre (N.DIALOG) ;
			
			! SERVER SETTING
			CALL Common.ServerSettingControl (N.DIALOG) ;
			
			! LEX DB LOAD
			CALL Common.Lex (N.DIALOG) ;
			
			! GRAMMAR LOAD
			CALL Common.GrammarLoad (N.DIALOG) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_GUIDANCE THEN
		BEGIN
			! PROMPT
			S.INNER.PROMPT.KINDLY.STR := S.INNER.PROMPT.KINDLY.STR ;
			S.INNER.PROMPT.SIMPLY.STR := S.INNER.PROMPT.SIMPLY.STR ;
			S.INNER.PROMPT.DISPLY.STR := S.INNER.PROMPT.DISPLY.STR ;
			
			S.INNER.LOG.STRING ~= N.SCREEN.PROMPT ;
			LOG "LP LOG : Pre.NoRecogBack - N.SCREEN.PROMPT = " + S.INNER.LOG.STRING ;
			
			! GUIDANCE PROMPT CONTROL
			CALL Common.GuidancePromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.GuidanceOnScreenText () ;
		END	
EOP

