			N.DIALOG := $N.STATUS.DIALOG ;
			S.INNER.LOG.STRING ~= N.DIALOG ;
			LOG "LP LOG : Pre.MainmenuSelect - N.DIALOG = " + S.INNER.LOG.STRING ;
			
			! CONTROL
			CALL Common.ControlPre (N.DIALOG) ;
			
			! MULTI MODAL KEYWORD
			CALL Common.ContextKeywords(N.DIALOG) ;
			
			! TIMEOUT PARAMETER
			CALL System.Param.Timeout (N.DIALOG) ;
			
			! BARGE IN SETTING
			CALL System.BargeinSettingControl (N.DIALOG) ;
			
			! SYSTEM CALL
			CALL Common.SystemCallPre (N.DIALOG) ;
			
			! SERVER SETTING
			CALL Common.ServerSettingControl (N.DIALOG) ;
			
			! LEX DB LOAD
			CALL Common.Lex (N.DIALOG) ;
			
			! GRAMMAR LOAD
			CALL Common.GrammarLoad (N.DIALOG) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_GUIDANCE THEN
		BEGIN
			! PROMPT
			IF S.PRE.RESULT.FIX.CID = "\@N_TAG_PHONE_CALL_ID" THEN
				BEGIN
					IF N.BT.PHONE.CONNECT = 1 THEN
						BEGIN
							IF N.BT.PHONEBOOK.DOWNLOADABLE = 0 THEN
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K2_1_1_02" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S2_1_1_02" ; 
									S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_2_1_01" ;
								END
							ELSE ! N.BT.PHONEBOOK.DOWNLOADABLE = 1 | 3
								BEGIN
									IF S.RESULT.FIX.CID = "\@N_TAG_NO_ID" THEN
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K2_1_1_02" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S2_1_1_02" ; 
											S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_2_1_01" ;
										END
									ELSE
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K2_1_1_03" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S2_1_1_03" ;
											S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_2_1_02" ;
										END
								END
						END
					ELSE ! N.BT.PHONE.CONNECT = 0 HFP not support
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K2_1_0_01" ;
