				END
			ELSE
				BEGIN
					IF S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_ADDRESS_ID" | S.PRE.RESULT.FIX.CID = "\@N_TAG_FIND_ADDRESS_STATE_ID" THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_2_1_01" + " " + S.NAVI.CURRENT.REGION ;
