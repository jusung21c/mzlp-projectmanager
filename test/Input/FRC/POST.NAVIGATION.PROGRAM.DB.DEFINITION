							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_8_01" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_8_01" ; 
							S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_8_01" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_EXIT ;
						END
					ELSE
						BEGIN
							IF N.NAVI.ROADASSISTANCE.NUM = 1 THEN
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_2_03" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_2_03" ; 
									S.INNER.PROMPT.DISPLY.STR := "" ;
									
									IF N.NAVI.DESTINATION.SETTING = 0 THEN
										BEGIN
											IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_SELECT ;
												END
											ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_SELECT ;
												END
										END
									ELSE ! N.NAVI.DESTINATION.SETTING = 1
										BEGIN
											IF N.NAVI.PHONE.NUMBER.EXIST = 0 THEN
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_WAY_SELECT ;
												END
											ELSE ! N.NAVI.PHONE.NUMBER.EXIST = 1
												BEGIN
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_CALL_WAY_SELECT ;
												END
										END
								END
							ELSE ! N.NAVI.ROADASSISTANCE.NUM > 1
								BEGIN
									S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_4_02" ;
									S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_4_02" ; 
									S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_4_02" ;
									$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_LIST ;
								END
						END
				END
		END
EOP

