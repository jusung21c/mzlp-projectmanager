! 인식된 시설명의 ID로 GENRE CODE TABLE에서 GENRE CODE를 검색하는 프로시저
JINIE LpProc.CategoryGenreCode ()

	TEMPORARY NUMBER N.TEMP := 2 ;
	
	S.GENRE.CODE := "" ;
	
	! GENRE CODE
	IF S.RESULT.CID = "\@N_TAG_CATEGORY_NAME_BIG_ID" THEN ! MULTI-MODAL BIG
		BEGIN
			N.TEMP := 2 ;
			WHILE N.TEMP < N.GENRE.COUNT.BIG DO ! 2<=10
				BEGIN
					IF S.GENRE.TABLE.BIG[N.TEMP][2] = S.SLOT.FIRST.VALUE THEN
						BEGIN
							S.GENRE.CODE := S.GENRE.CODE + S.GENRE.TABLE.BIG[N.TEMP][1] ;
						END
					N.TEMP := N.TEMP + 1 ;
				END
		END
	ELSE
		BEGIN
			IF S.RESULT.CID = "\@N_TAG_CATEGORY_NAME_MID_ID" THEN	! MULTI-MODAL MID
				BEGIN
					N.TEMP := 2 ;
					WHILE N.TEMP < N.GENRE.COUNT.MID DO
						BEGIN
							IF S.GENRE.TABLE.MID[N.TEMP][2] = S.SLOT.FIRST.VALUE THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.CODE + S.GENRE.TABLE.MID[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1 ;
						END
				END
			ELSE	! FIND <CATEGORY> : BIG MID SMALL 
				BEGIN
					N.TEMP := 2 ;
					WHILE N.TEMP < N.GENRE.COUNT.BIG	DO ! 2<=10
						BEGIN
							IF S.GENRE.TABLE.BIG[N.TEMP][2] = S.SLOT.FIRST.VALUE THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.CODE + S.GENRE.TABLE.BIG[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1;
						END
						
					N.TEMP := 2 ;
					WHILE N.TEMP < N.GENRE.COUNT.MID	DO
						BEGIN
							IF S.GENRE.TABLE.MID[N.TEMP][2] = S.SLOT.FIRST.VALUE THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.CODE + S.GENRE.TABLE.MID[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1;
						END
						
					N.TEMP := 2 ;
					WHILE N.TEMP < N.GENRE.COUNT.SMALL	DO
						BEGIN
							IF S.GENRE.TABLE.SMALL[N.TEMP][2] = S.SLOT.FIRST.VALUE THEN
								BEGIN
									S.GENRE.CODE := S.GENRE.CODE + S.GENRE.TABLE.SMALL[N.TEMP][3] ;
								END
							N.TEMP := N.TEMP + 1;
						END
				END
		END

	LOG "LPLOG : LpProc.CategoryGenreCode " + S.GENRE.CODE ;
EOP

