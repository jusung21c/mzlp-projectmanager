! "주변 검색" 명령어 처리 프로시저
JINIE LpProc.FindPoi ()

	IF N.NAVI.SDCARD.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.NAVI.SDCARD.EXIST = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ; 
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK;
				END
			ELSE
				BEGIN
					IF N.VENDER.VR.MODE = 1 THEN
						BEGIN
							IF N.NETWORK.SUBSCRIBED = 1 THEN
								BEGIN
									IF N.NETWORK.SETTING = 1 THEN
										BEGIN
											IF $S.STT.RESULT.OFFLINE.INTENTION[1][1] = "WaitServerCategory" THEN
												BEGIN
													CALL Common.ServerVRExit () ;
												END
											ELSE
												BEGIN
													S.PRE.RESULT.FIX.CID := "\@N_TAG_FIND_SERVER_ID" ;
													S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_09" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_09" ;
													S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_3_1_0_52" + "\n" + "\@S_SCREEN_PROMPT_LID_3_1_0_53" ;
													$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_SERVER ;
												END
										END
									ELSE
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ; 
											S.INNER.PROMPT.DISPLY.STR := "" ;
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_BIG_CATEGORY ;
										END
								END
							ELSE
								BEGIN
									IF N.SERVER.EXPIRED.PROMPT.CHECK = 1 THEN
										BEGIN
											IF N.VENDER.NETWORK = 1 THEN
												BEGIN
													S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_13" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_13" ;
												END
											ELSE
												BEGIN
													S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_1_2_14" ;
													S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_1_2_14" ;
												END
											S.INNER.PROMPT.DISPLY.STR := "" ;
											$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_BIG_CATEGORY ;
										END
									ELSE
										BEGIN
											S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
											S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ; 
											S.INNER.PROMPT.DISPLY.STR := "" ;
											$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_BIG_CATEGORY ;
										END
								END
						END
					ELSE
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ; 
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_FIND_BIG_CATEGORY ;
						END
				END
		END
