JINIE Post.NavigationMain ()

	! CHECK MULTI-MODAL INTERRUPT
	CALL Common.CheckInterrupt () ;
	
	IF $N.STATUS.PROCESS = @N_PROCESS_RULE & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			! CONTROL PROCESS & PROGRAM
			CALL Common.ControlPost () ;
			
			! SET REFERENCE DB & REFERENCE KEY
			S.INTENT.VALUE := $S.STT.RESULT.INTENTION[1][1] ; ! INTENTION SAVED
			S.SLOT.FIRST.TAG := $S.STT.RESULT.SLOT[1][1] ; ! SLOT1 TAG SAVED
			S.SLOT.SECOND.TAG := $S.STT.RESULT.SLOT[1][4] ; ! SLOT2 TAG SAVED
			S.SLOT.THIRD.TAG := $S.STT.RESULT.SLOT[1][7] ; ! SLOT3 TAG SAVED
			
			N.REF.DB := @N_REFERENCE_INFO ;
			N.REF.ID := 11 ;
			S.REF.KEY := S.INTENT.VALUE + S.SLOT.FIRST.TAG + S.SLOT.SECOND.TAG + S.SLOT.THIRD.TAG ;
			
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_NAVIGATION_SERVER" THEN
				BEGIN
					! SERVER INFO 전달
					CALL Common.ExtraInfo () ;
					
					IF $S.STT.RESULT.SLOT[1][1] = "" THEN
						BEGIN
							S.SLOT.FIRST.TAG := "" ;
						END
					ELSE
						BEGIN
							S.SLOT.FIRST.TAG := "Slot" ;
						END
					
					S.DOMAIN.VALUE := $S.STT.RESULT.ONLINE.DOMAIN ;	! SERVER DOMAIN
					
					N.REF.DB := @N_REFERENCE_INFO ;
					N.REF.ID := 11 ;
					S.REF.KEY := S.DOMAIN.VALUE + S.SLOT.FIRST.TAG ;
				END
			ELSE
				BEGIN
					IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_SERVER_REJECTION" THEN ! 삭제 확인 필요
						BEGIN 
							N.REF.DB := @N_REFERENCE_INFO ;
							N.REF.ID := 1 ;
							S.REF.KEY := "\@N_TAG_UNSUPPORTED_SERVER_ID" ;
						END
					ELSE ! EMBEDDED
						BEGIN
							IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_BIG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_MID" THEN
								BEGIN ! ONLY MULTI-MODAL
									N.REF.DB := @N_REFERENCE_CATEGORY ;
									N.REF.ID := 1 ;
									S.REF.KEY := $S.RESULT.ID[1] ;
								END
							ELSE
								BEGIN
									IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_FIND_POI" & $S.STT.RESULT.SLOT[1][1] = "<SLOT_CATEGORY_NAME>" THEN ! FIND <CATEGORY>, <CATEGORY>
										BEGIN
											N.REF.DB := @N_REFERENCE_CATEGORY ;
											N.REF.ID := 2 ;
											S.REF.KEY := $S.STT.RESULT.SLOT[1][2] ;
										END
									ELSE
										BEGIN
											IF $S.STT.RESULT.SLOT[1][1] = "<SLOT_STATE_NAME>" THEN	! Find <STATE_NAME>
												BEGIN
													N.REF.DB := @N_REFERENCE_INFO ;
													N.REF.ID := 2 ;
													S.REF.KEY := $S.STT.RESULT.SLOT[1][2] ;
												END
											ELSE
												BEGIN
													! 가능한 패턴 : <SLOT_HOUSE_NUMBER> <STREET_CITY> <SLOT_STATE_NAME>, <SLOT_HOUSE_NUMBER> <STREET_CITY>, <STREET_CITY> <SLOT_STATE_NAME>, <STREET_CITY>
													IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_VDE_LOCATION THEN
														BEGIN
															IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_HOUSE_NUMBER" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_STREET_NAME" THEN
																BEGIN
																	N.REF.DB := @N_REFERENCE_INFO ;
																	N.REF.ID := 1 ;
																	S.REF.KEY := "\@N_TAG_FIND_FULL_ADDRESS_ID" ;
																END
														END
													! 가능한 패턴 : <STREET_CITY>
													IF $N.STATUS.DIALOG = @N_DIALOG_NAVI_VDE_STREETCITY_STEP & $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_STREET_NAME" THEN
														BEGIN
															N.REF.DB := @N_REFERENCE_INFO ;
															N.REF.ID := 1 ;
															S.REF.KEY := "\@N_TAG_USER_STREET_CITY_ID" ;
														END
												END
										END
								END
						END
				END
				
			CALL Common.ReferenceKey (N.REF.DB, N.REF.ID, S.REF.KEY) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_RESULT & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			! ART
			CALL Common.ARTIntentionSetting () ;
			
			! GET N-best Result From Reference DB
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_NAVIGATION_SERVER" THEN
				BEGIN
					CALL Common.ReferenceOneBestResultNavigationServer () ; ! Common.ReferenceOneBestResult사용 Common.SetActionString에서 분기 추가 검토
				END
			ELSE
				BEGIN
					IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_BIG" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_MID" THEN
						BEGIN ! ONLY MULTI-MODAL
							CALL Common.ReferenceOneBestResultPoiCategory () ;
							
							IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_CATEGORY_NAME_MID" THEN
								BEGIN
									S.INNER.CATEGORY.INFO := S.ACTION.STRING ; ! 인식된 카테고리 정보 저장
								END
						END
					ELSE
						BEGIN
							IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_FIND_POI" & $S.STT.RESULT.SLOT[1][1] = "<SLOT_CATEGORY_NAME>" THEN
								BEGIN
									CALL Common.ReferenceOneBestResultPoiCategory () ;
									
									IF $N.STATUS.DIALOG = @N_DIALOG_MAINMENU | $N.STATUS.DIALOG = @N_DIALOG_MAINMENU_HELP_NAVIGATION | $N.STATUS.DIALOG = @N_DIALOG_NAVI_FIND_MID_CATEGORY THEN
										BEGIN
											S.INNER.CATEGORY.INFO := S.ACTION.STRING ; ! 인식된 카테고리 정보 저장
										END
								END
							ELSE
								BEGIN
									! 가능한 패턴 : <SLOT_HOUSE_NUMBER> <STREET_CITY> <SLOT_STATE_NAME>, <SLOT_HOUSE_NUMBER> <STREET_CITY>, <STREET_CITY> <SLOT_STATE_NAME>, <STREET_CITY>
									IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_HOUSE_NUMBER" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_STREET_NAME" | $S.STT.RESULT.SLOT[1][1] = "<SLOT_HOUSE_NUMBER>" THEN ! <CITY&STREET><HOUSE_NUMBER>
										BEGIN
											CALL LpProc.FindFullAddressRuleProcess () ;
										END
									ELSE
										BEGIN
											IF $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_NEAR_CURRENT_POSITION" | $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_NEAR_DESTINATION" | $S.RULE.LEXEMES[1][2]  = "\@N_LEXEME_ALONG_ROUTE" THEN
												BEGIN
													S.ACTION.STRING := S.INNER.CATEGORY.INFO ; ! 인식된 카테고리 정보 전달
												END
												
											CALL Common.ReferenceOneBestResult () ;
										END
								END
						END
				END
				
			! SET ACTION-VALUE
			CALL Common.SetActionValue () ;
			
			! SET RULE-TAG
			CALL Common.SetPreRuleTag () ;
			
			! SYSTEM CALL
			N.CID ~= S.RESULT.FIX.CID ;
			CALL Common.SystemCallPost (N.CID) ;
			
			! CALL POST PROGRAM
			IF S.RESULT.FIX.CID = "\@N_TAG_SHOW_MAP_ID" THEN
				BEGIN
					CALL LpProc.ShowMap () ;
				END

			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_ADDRESS_ID" THEN
				BEGIN
					CALL LpProc.FindAddress () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_ADDRESS_STATE_ID" THEN
				BEGIN
					CALL LpProc.FindAddressState () ;
				END
				
			IF N.DIALOG = @N_DIALOG_NAVI_VDE_LOCATION & S.SLOT.FIRST.TAG = "<SLOT_STATE_NAME>" |
			$S.RULE.LEXEMES[1][1] = "\@N_LEXEME_STATE_NAME" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_STATE_NAME" THEN
				BEGIN ! MULTI-MOCAL | RECOG
					CALL LpProc.StateName () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_USER_HOUSE_NUMBER_ID" THEN
				BEGIN
					CALL LpProc.UserHouseNumber () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_FULL_ADDRESS_ID" THEN
				BEGIN
					CALL LpProc.FindFullAddress () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_USER_STREET_CITY_ID" THEN
				BEGIN
					CALL LpProc.UserStreetCity () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ID" THEN
				BEGIN
					CALL LpProc.FindPoi () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_FIND_CATEGORY_ID" THEN
				BEGIN
					CALL LpProc.FindPoiCategory () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_CATEGORY_NAME_BIG_ID" THEN
				BEGIN
					CALL LpProc.CategoryNameBig () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_CATEGORY_NAME_MID_ID" THEN
				BEGIN
					CALL LpProc.CategoryNameMid () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NEAR_CURRENT_POSITION_ID" THEN
				BEGIN
					CALL LpProc.NearCurrentPosition () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NEAR_DESTINATION_ID" THEN
				BEGIN
					CALL LpProc.NearDestination () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ALONG_ROUTE_ID" THEN
				BEGIN
					CALL LpProc.AlongRoute () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_POINT_ID" THEN
				BEGIN
					CALL LpProc.PreviousPoint () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_DESTINATION_ID" THEN
				BEGIN
					CALL LpProc.PreviousDestination () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_SEARCH_ID" THEN
				BEGIN
					CALL LpProc.PreviousSearch () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_START_ID" THEN
				BEGIN
					CALL LpProc.PreviousStart () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TRAFFIC_INFO_ID" THEN
				BEGIN
					CALL LpProc.TrafficInfo () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_ON_ID" THEN
				BEGIN
					CALL LpProc.TurnGuidanceOn () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_TURN_GUIDANCE_OFF_ID" THEN
				BEGIN
					CALL LpProc.TurnGuidanceOff () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DESTINATION_INFO_ID" THEN
				BEGIN
					CALL LpProc.DestinationInfo () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ROUTE_OPTION_ID" THEN
				BEGIN
					CALL LpProc.RouteOption () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_SHOW_ROUTE_ID" THEN
				BEGIN
					CALL LpProc.ShowRoute () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ADD_WAYPOINT_ID" THEN
				BEGIN
					CALL LpProc.AddWaypoint () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_GO_HOME_ID" THEN
				BEGIN
					CALL LpProc.GoHome () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_GO_TO_OFFICE_ID" THEN
				BEGIN
					CALL LpProc.GoToOffice () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_EMERGENCY_SERVICES_ID" THEN
				BEGIN
					CALL LpProc.EmergencyServices () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_POLICE_STATION_ID" THEN
				BEGIN
					CALL LpProc.PoliceStation () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_HOSPITAL_ID" THEN
				BEGIN
					CALL LpProc.Hospital () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DEALERSHIP_ID" THEN
				BEGIN
					CALL LpProc.Dealership () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ROAD_ASSISTANCE_ID" THEN
				BEGIN
					CALL LpProc.RoadAssistance () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_CANCEL_ROUTE_ID" THEN
				BEGIN
					CALL LpProc.CancelRoute () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_DELETE_WAYPOINT_ID" THEN
				BEGIN
					CALL LpProc.DeleteWayPoint () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_NONE_OF_THEM_ID" THEN
				BEGIN
					CALL LpProc.NoneOfThem () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_STREET_CITY_STEP_ID" THEN
				BEGIN
					CALL LpProc.StreetCityStep () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_HOUSE_NUMBER_STEP_ID" THEN
				BEGIN
					CALL LpProc.HouseNumberStep () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ADDRESS_STEP_EDIT_ID" THEN
				BEGIN
					CALL LpProc.AddressStepEdit () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_HOUSE_NUMBER_BACK_STEP_ID" THEN
				BEGIN
					CALL LpProc.HouseNumberBackStep () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ZOOM_OUT_ID" THEN
				BEGIN
					CALL LpProc.ZoomOut () ;
				END
				
			IF S.RESULT.FIX.CID = "\@N_TAG_ZOOM_IN_ID" THEN
				BEGIN
					CALL LpProc.ZoomIn () ;
				END
				
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_NAVIGATION_SERVER" | $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_WAIT_SERVER" THEN
				BEGIN
					IF S.SLOT.FIRST.TAG = "Call" | S.SLOT.FIRST.TAG = "Distance" THEN ! 해당 분기 필요 검토 (@N_LEXEME_SERVER_REJECTION으로 빠지니까...)
						BEGIN
							CALL Common.EmbeddedRejection () ;
						END
					ELSE
						BEGIN
							IF S.RESULT.FIX.CID = "\@N_TAG_FIND_SERVER_ID"THEN
								BEGIN
									CALL LpProc.FindServer () ;
								END
								
							IF S.RESULT.FIX.CID = "\@N_TAG_FIND_POI_ADDRESS_SERVER_ID"THEN
								BEGIN
									CALL LpProc.FindPoiAddressServer () ;
								END
								
							IF S.RESULT.FIX.CID = "\@N_TAG_POI_ADDRESS_SERVER_ID"THEN
								BEGIN
									CALL LpProc.PoiAddressServer () ;
								END
						END
				END
				
			IF $S.RULE.LEXEMES[1][2] = "\@N_LEXEME_SERVER_REJECTION" THEN
				BEGIN
					CALL LpProc.UnsupportedServerRejection () ;
				END
				
			! REPEAT CONTROL
			CALL Common.RepeatControl () ;
			
			! FEEDBACK PROMPT CONTROL
			CALL Common.FeedbackPromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.FeedbackOnScreenText () ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_TRANSITION & $N.STATUS.PROGRAM = @N_FLAG_ON THEN
		BEGIN
			! VR 종료 후, MAINMENU Dialog 진입 시 정상 Prompt 출력을 위하여 초기화
			IF $N.TRANSITION.DIALOG = @N_DIALOG_STOP | $N.TRANSITION.DIALOG = @N_DIALOG_EXIT THEN
				BEGIN
					S.RESULT.FIX.CID := "" ;
				END
		END
EOP

! "지도 보기" 명령어 처리 프로시저
JINIE LpProc.ShowMap ()

	IF N.NAVI.SDCARD.EXIST = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.NAVI.SDCARD.EXIST = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ;
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK;
				END
			ELSE
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_11_01" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_11_01" ;
					S.INNER.PROMPT.DISPLY.STR := "" ; 
					N.EXIT.ACTION.TRANSITION := 19101;
					$N.CONTROL.PROCESS.FEEDBACK := @N_FLAG_ON ;
					$N.TRANSITION.DIALOG := @N_DIALOG_STOP ; !silence 
				END
		END
EOP

JINIE LpProc.FindAddress ()

	IF N.NAVI.SDCARD.EXIST = 200000 | N.NAVI.CURRENT.REGION = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.NAVI.SDCARD.EXIST = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_7_0_02" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_7_0_02" ; 
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_7_0_06" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_BACK ;
				END
			ELSE
				BEGIN
					IF N.NAVI.CURRENT.REGION <> 156 & N.NAVI.CURRENT.REGION <> 163  THEN
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_3_1_05" + " " + "\@S_DIALOG_PROMPT_LID_K3_2_1_07" ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_3_1_05" + " " + "\@S_DIALOG_PROMPT_LID_S3_2_1_07" ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_VDE_LOCATION ;
						END
					ELSE
						BEGIN
							S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_2_1_01" + " " + S.NAVI.CURRENT.REGION ;
							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_2_1_01" + " " + S.NAVI.CURRENT.REGION ; 
							S.INNER.PROMPT.DISPLY.STR := "" ;
							$N.TRANSITION.DIALOG := @N_DIALOG_NAVI_VDE_LOCATION ;
						END
				END
		END
EOP

! Address Search, Display String 처리
JINIE LpProc.FindFullAddressRuleProcess ()
	
	N.STORE.NBEST := 0 ;
	N.INNER.CNC.FLAG := 0 ; !C&C 확인 FLAG
	N.INNER.ROW := 1 ; ! 인식 어휘에 대한 행 확인
	N.INNER.COLUMN := 1 ; ! 인식 어휘에 대한 열 확인
	N.INNER.DUPLICATION.COUNT := 0 ; ! 중복 확인 Count
	N.INNER.DUPLICATION.FLAG := 0 ; ! 중복 확인 Flag
	S.INNER.CITY.STRING := "" ; ! 인식 주소 중 CITY STRING 저장
	S.INNER.STREET.STRING := "" ; ! 인식 주소 중 STREET STRING 저장
	S.INNER.HOUSENUMBER.STRING := "" ; ! 인식 주소 중 HOUSE NUMBER STRING 저장
	N.INNER.TEMP.HOUSENUMBER.NUMBER := 0 ; ! 인식 된 HOUSE NUMBER ID에 대한 중간 처리 값 저장
	S.INNER.TEMP.HOUSENUMBER.STRING := "" ; ! 인식 된 HOUSE NUMBER ID에 대한 중간 처리 값 저장
	N.RESULT.CONUT.NUM := 0 ; ! 인식 어휘에 대한 NBEST 개수 확인
	
	IF N.DIALOG = @N_DIALOG_NAVI_VDE_LOCATION THEN
		BEGIN
			WHILE N.INNER.ROW <= $N.RESULT.RESULTS DO
				BEGIN
					N.RESULT.ID := 0 ;
					N.INNER.COLUMN := 1 ;
					N.INNER.DUPLICATION.FLAG := 0 ;
					N.INNER.CNC.FLAG := 0 ;
					S.INNER.CITY.STRING := "" ;
					S.INNER.STREET.STRING := "" ;
					S.INNER.HOUSENUMBER.STRING := "" ;
					
					CALL LpProc.AddressProcess () ;
					
					IF N.INNER.CNC.FLAG = 0 THEN
						BEGIN
							! 인식된 주소 저장
							S.INNER.DISPLAY.STRING := S.INNER.HOUSENUMBER.STRING + " " + S.INNER.STREET.STRING + " " + S.INNER.CITY.STRING ; ! 100 Main Strret New York
							S.INNER.SEARCH.STRING := S.INNER.HOUSENUMBER.STRING + "_" + S.INNER.STREET.STRING + "_" + S.INNER.CITY.STRING ; ! 100_Main Strret_New York
							
							CALL Common.ReferenceNBestResultAddressName () ;
						END
						
					N.INNER.ROW := N.INNER.ROW + 1 ;
				END
			
			! NBEST LIST PROMPT
			S.INNER.NAVI.NBESTLIST.NUMBER ~= N.RESULT.CONUT.NUM ;
			LOG "LPLOG - S.INNER.NAVI.NBESTLIST.NUMBER : " + S.INNER.NAVI.NBESTLIST.NUMBER ;
		END
		
	IF N.DIALOG = @N_DIALOG_NAVI_VDE_STREETCITY_STEP THEN
		BEGIN
			CALL LpProc.AddressProcess () ;
			
			S.INNER.DISPLAY.STRING := S.INNER.STREET.STRING + " " + S.INNER.CITY.STRING ; ! Main Strret New York
			S.INNER.SEARCH.STRING := S.INNER.STREET.STRING + "_" + S.INNER.CITY.STRING ; ! Main Strret_New York
			
			CALL Common.ReferenceNBestResultAddressName () ;
		END
		
	IF N.DIALOG = @N_DIALOG_NAVI_VDE_HOUSE_NUMBER_STEP THEN
		BEGIN
			CALL LpProc.AddressProcess () ;
			
			S.INNER.DISPLAY.STRING := S.INNER.HOUSENUMBER.STRING ; ! 100
			S.INNER.SEARCH.STRING := S.INNER.HOUSENUMBER.STRING ; ! 100
			
			CALL Common.ReferenceNBestResultAddressName () ;
		END
	
EOP

JINIE LpProc.AddressProcess ()

	WHILE $S.RESULT.ID[N.INNER.ROW][N.INNER.COLUMN] <> "" DO
		BEGIN
			N.RESULT.ID ~= $S.RESULT.ID[N.INNER.ROW][N.INNER.COLUMN] ;
			
			! HOUSE NUMBER 범위 : 63300001 ~ 63399999, STREET 범위 : 636000001 ~ 636999999, CITY 범위 : 63500001 ~ 63599999
			IF ( 63300000 <= N.RESULT.ID & N.RESULT.ID < 63400000 ) THEN ! HOUSE NUMBER
				BEGIN
					N.INNER.TEMP.HOUSENUMBER.NUMBER := N.RESULT.ID - 63300000 ;
					S.INNER.TEMP.HOUSENUMBER.STRING ~= N.INNER.TEMP.HOUSENUMBER.NUMBER ;
					
					IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9901 THEN
						BEGIN
							S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "-" ;
						END
					ELSE
						BEGIN
							IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9902 THEN
								BEGIN
									S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "D" ;
								END
							ELSE
								BEGIN
									IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9903 THEN
										BEGIN
											S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "E" ;
										END
									ELSE
										BEGIN
											IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9904 THEN
												BEGIN
													S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "N" ;
												END
											ELSE
												BEGIN
													IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9905 THEN
														BEGIN
															S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "S" ;
														END
													ELSE
														BEGIN
															IF N.INNER.TEMP.HOUSENUMBER.NUMBER = 9906 THEN
																BEGIN
																	S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + "W" ;
																END
															ELSE ! NUMBER
																BEGIN
																	S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING + S.INNER.TEMP.HOUSENUMBER.STRING  ;
																END
														END
												END
										END
								END
						END
				END
			ELSE
				BEGIN
					IF ( 636000000 < N.RESULT.ID & N.RESULT.ID < 637000000) THEN ! STREET 
						BEGIN
							IF S.INNER.HOUSENUMBER.STRING = "" THEN
								BEGIN
									S.INNER.STREET.STRING := $S.RULE.LEXICONS[N.INNER.ROW][2] ;
								END
							ELSE
								BEGIN
									S.INNER.STREET.STRING := $S.RULE.LEXICONS[N.INNER.ROW][3] ;
								END
						END
					ELSE
						BEGIN
							IF ( 63500000 < N.RESULT.ID & N.RESULT.ID < 63600000) THEN ! CITY
								BEGIN
									IF S.INNER.HOUSENUMBER.STRING = "" THEN
										BEGIN
											S.INNER.CITY.STRING := $S.RULE.LEXICONS[N.INNER.ROW][3] ;
										END
									ELSE
										BEGIN
											S.INNER.CITY.STRING := $S.RULE.LEXICONS[N.INNER.ROW][4] ;
										END
								END
							ELSE ! C&C | STATE
								BEGIN
									IF ( 634000 < N.RESULT.ID & N.RESULT.ID < 635000) THEN ! STATE
										BEGIN
											IF N.INNER.COLUMN = 1 THEN ! ONLY STATE : TAB
												BEGIN
													N.INNER.CNC.FLAG := 1 ;
												END
											ELSE
												BEGIN
													S.INNER.HOUSENUMBER.STRING := S.INNER.HOUSENUMBER.STRING ;
													S.INNER.STREET.STRING := S.INNER.STREET.STRING ;
													S.INNER.CITY.STRING := S.INNER.CITY.STRING ;
												END
										END
									ELSE
										BEGIN
											N.INNER.CNC.FLAG := 1 ;
										END
								END
						END
				END
			N.INNER.COLUMN := N.INNER.COLUMN + 1 ;
		END
EOP

