! [?] 버튼 처리 프로시저
JINIE LpProc.ManualQuestionMark ()

	IF N.SYSTEM.DRIVING = 200000 THEN
		BEGIN
			CALL Common.FeedbackBasicSetting () ;
		END
	ELSE
		BEGIN
			IF N.SYSTEM.DRIVING = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "" ;
					S.INNER.PROMPT.SIMPLY.STR := "" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_NO_RECOG_DRIVING ;
				END
			ELSE
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_11_07" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_11_07" ;
					S.INNER.PROMPT.DISPLY.STR := "\@S_SCREEN_PROMPT_LID_1_11_04" ;
					S.INNER.TEMP.STRING := "\@N_TAG_DRIVING_BACK_ID" ;
					$N.TRANSITION.DIALOG := @N_DIALOG_REPEAT ;
				END
		END
EOP
