			! MULTI MODAL KEYWORD
			CALL Common.ContextKeywordsSdCard (N.DIALOG) ;
			
			! TIMEOUT PARAMETER
			CALL System.Param.Timeout (N.DIALOG) ;
			
			! BARGE IN SETTING
			CALL System.Param.Bargein (N.DIALOG) ;
			
			! SERVER SETTING
			CALL Common.ServerSettingControl (N.DIALOG) ;
			
			! LEX DB LOAD
			CALL Common.LexSdCard (N.DIALOG) ;
			
			! GRAMMAR LOAD
			CALL Common.GrammarLoadSdCard (N.DIALOG) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_GUIDANCE THEN
		BEGIN
			! PROMPT
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
			
			IF S.RESULT.FIX.CID = "\@N_TAG_NEXT_PAGE_ID" & N.GLOBAL.LISTITEM.REMAIN = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_13_03" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_13_03" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
				END
			
			IF S.RESULT.FIX.CID = "\@N_TAG_PREVIOUS_PAGE_ID" & N.GLOBAL.LISTITEM.REMAIN = 0 THEN
				BEGIN
					S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K1_13_02" ;
					S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S1_13_02" ;
					S.INNER.PROMPT.DISPLY.STR := "" ;
				END
			
			! GUIDANCE PROMPT CONTROL
			CALL Common.GuidancePromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.GuidanceOnScreenText () ;
		END
EOP