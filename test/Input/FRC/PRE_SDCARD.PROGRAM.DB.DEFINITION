! POI Big Category 리스트 화면
JINIE Pre.NaviFindBigCategory ()

	IF $N.STATUS.PROCESS = @N_PROCESS_INIT THEN
		BEGIN
			! CURRENT DIALOG SETTING
			N.DIALOG := $N.STATUS.DIALOG ;
			S.INNER.LOG.STRING ~= N.DIALOG ;
			LOG "LP LOG : CURRENT DIALOG - N.DIALOG = " + S.INNER.LOG.STRING ;
			
			! CONTROL
			CALL Common.ControlPre (N.DIALOG) ;
			
			! SYSTEMCALL
			CALL Common.SystemCallPreSdCard (N.DIALOG) ;
			
			! MULTI MODAL KEYWORD
			CALL Common.ContextKeywordsSdCard (N.DIALOG) ;
			
			! TIMEOUT PARAMETER
			CALL System.Param.Timeout (N.DIALOG) ;
			
			! BARGE IN SETTING
			CALL System.Param.Bargein (N.DIALOG) ;
			
			! SERVER SETTING
			CALL Common.ServerSettingControl (N.DIALOG) ;
			
			! LEX DB LOAD
			CALL Common.LexSdCard (N.DIALOG) ;
			
			! GRAMMAR LOAD
			CALL Common.GrammarLoadSdCard (N.DIALOG) ;
		END
		
	IF $N.STATUS.PROCESS = @N_PROCESS_GUIDANCE THEN
		BEGIN
			! PROMPT
			S.INNER.PROMPT.KINDLY.STR := "\@S_DIALOG_PROMPT_LID_K3_6_1_01" ;
			S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S3_6_1_01" ;
			S.INNER.PROMPT.DISPLY.STR := "" ;
		
			! GUIDANCE PROMPT CONTROL
			CALL Common.GuidancePromptControl () ;
			
			! ON SCREEN TEXT
			CALL Common.GuidanceOnScreenText () ;
		END
EOP

! POI Mid Category 리스트 화면
