! DIALOG에서 사용하는 GRAMMAR 및 ACTIVATE RULE 설정
JINIE Common.GrammarLoad (NUMBER N.DIALOG)

	S.GRAMMAR := "" ;
	S.ACTIVATION := "" ;
	
	S.INNER.LOG.STRING ~= N.NAVI.SDCARD.EXIST ;
	LOG "LPLOG : N.NAVI.SDCARD.EXIST - " + S.INNER.LOG.STRING ;			
	
	S.INNER.LOG.STRING ~= N.BT.PATTERN.MODE ;
	LOG "LPLOG : N.BT.PATTERN.MODE - " + S.INNER.LOG.STRING ;		
							
	S.INNER.LOG.STRING ~= N.BT.CONTACT.NUMBER ;
	LOG "LPLOG : N.BT.CONTACT.NUMBER - " + S.INNER.LOG.STRING ;

	S.INNER.LOG.STRING ~= N.BT.DP.NUMBER ;
	LOG "LPLOG : N.BT.DP.NUMBER - " + S.INNER.LOG.STRING ;
	
	IF  N.NAVI.CURRENT.REGION > 165 | ( N.NAVI.CURRENT.REGION < 100 &  N.NAVI.CURRENT.REGION <> 0 ) THEN
		BEGIN
