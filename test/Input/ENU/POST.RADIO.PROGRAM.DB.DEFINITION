							S.INNER.PROMPT.SIMPLY.STR := "\@S_DIALOG_PROMPT_LID_S4_3_1_01" + "\\\\pause=100\\\\" + S.SLOT.RESULT ;
							S.INNER.PROMPT.DISPLY.STR := "" ;
