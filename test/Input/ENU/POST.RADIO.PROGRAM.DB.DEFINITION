JINIE LpProc.SiriusxmChannelNumbersSlot ()
	
	TEMPORARY NUMBER N.TEMP := 1 ;
	TEMPORARY NUMBER N.SLOT.RESULT := 0 ;
	
	S.SLOT.RESULT := "" ;
	S.SLOT.ID := "" ;
	
	WHILE $S.RESULT.ID[1][N.TEMP] <> ""	DO
		BEGIN
			IF $S.RESULT.ID[1][N.TEMP] = "\@N_TAG_SIRIUSXM_CHANNEL_NUMBER_ID" | $S.RESULT.ID[1][N.TEMP] = "\@N_TAG_CHANNEL_NUMBER_ID" THEN
				BEGIN
					S.SLOT.RESULT := S.SLOT.RESULT ;
				END
			ELSE
				BEGIN
					IF $S.RESULT.ID[1][N.TEMP] = "100" & $S.RESULT.ID[1][N.TEMP+1] <> "" THEN	! 101 / 111
						BEGIN
							N.SLOT.RESULT ~= $S.RESULT.ID[1][N.TEMP+1];
							
							IF N.SLOT.RESULT < 10 THEN ! 101 
								BEGIN
									S.SLOT.RESULT := S.SLOT.RESULT + "10" ;
								END
							ELSE ! 111
								BEGIN
									S.SLOT.RESULT := S.SLOT.RESULT + "1" ;
								END
						END
					ELSE ! 100
						BEGIN
							S.SLOT.RESULT := S.SLOT.RESULT + $S.RESULT.ID[1][N.TEMP] ;
						END
				END
				
			N.TEMP := N.TEMP + 1 ;
		END
	
	S.SLOT.ID := S.SLOT.RESULT ;
EOP

