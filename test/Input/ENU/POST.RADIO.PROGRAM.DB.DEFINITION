! "에이엠 <Am_Freq>", "<Am_Freq>" 명령어 ID를 실제 주파수 변환 처리 프로시저
JINIE LpProc.AmFrequencySlot ()

	TEMPORARY NUMBER N.TEMP := 0 ;
	TEMPORARY NUMBER N.SLOT.FIRST := 0 ;
	TEMPORARY NUMBER N.SLOT.SECOND := 0 ;
	TEMPORARY NUMBER N.SLOT.THIRD := 0 ;
	TEMPORARY NUMBER N.SLOT.RESULT := 0 ;
	
	S.SLOT.RESULT := "" ;
	S.SLOT.ID := "" ;
	
	IF N.DIALOG = @N_DIALOG_MAINMENU | N.DIALOG = @N_DIALOG_MAINMENU_HELP_RADIO THEN
		BEGIN
			IF $S.RESULT.ID[1][1] = "\@N_TAG_AM_FREQUENCY_ID" THEN ! Command ID : AM 530
				BEGIN
					N.TEMP := 2 ;
				END
			ELSE ! 530 AM
				BEGIN
					N.TEMP := 1 ;
				END
			
			IF ( $S.RESULT.ID[1][N.TEMP+1] <> "" ) & ( $S.RESULT.ID[1][N.TEMP+1] <> "\@N_TAG_AM_FREQUENCY_ID" ) THEN 
				BEGIN
					IF ( $S.RESULT.ID[1][N.TEMP+2] <> "" ) & ( $S.RESULT.ID[1][N.TEMP+2] <> "\@N_TAG_AM_FREQUENCY_ID" ) THEN ! AM 1000 100 10 / 1000 100 10 AM
						BEGIN
							N.SLOT.FIRST ~= $S.RESULT.ID[1][N.TEMP] ;
							N.SLOT.SECOND ~= $S.RESULT.ID[1][N.TEMP+1] ;
							N.SLOT.THIRD ~= $S.RESULT.ID[1][N.TEMP+2] ;
						END
					ELSE ! AM 5 60 / 5 60 AM
						BEGIN
							N.SLOT.FIRST ~= $S.RESULT.ID[1][N.TEMP] ;
							N.SLOT.SECOND ~= $S.RESULT.ID[1][N.TEMP+1] ;
							N.SLOT.THIRD := 0 ;
						END
				END
			ELSE ! AM 1000 / 1000 AM
				BEGIN
					N.SLOT.FIRST ~= $S.RESULT.ID[1][N.TEMP] ;
					N.SLOT.SECOND := 0 ;
					N.SLOT.THIRD := 0 ;
				END
		END
	ELSE
		BEGIN
			IF $S.RESULT.ID[1][2] <> "" THEN 
				BEGIN
					IF $S.RESULT.ID[1][3] <> "" THEN ! 1000 100 10
						BEGIN
							N.SLOT.FIRST ~= $S.RESULT.ID[1][1] ;
							N.SLOT.SECOND ~= $S.RESULT.ID[1][2] ;
							N.SLOT.THIRD ~= $S.RESULT.ID[1][3] ;
						END
					ELSE ! 5 60
						BEGIN
							N.SLOT.FIRST ~= $S.RESULT.ID[1][1] ;
							N.SLOT.SECOND ~= $S.RESULT.ID[1][2] ;
							N.SLOT.THIRD := 0 ;
						END
				END
			ELSE ! 1000
				BEGIN
					N.SLOT.FIRST ~= $S.RESULT.ID[1][1] ;
					N.SLOT.SECOND := 0 ;
					N.SLOT.THIRD := 0 ;
				END
		END
		
	IF N.SLOT.FIRST < 100 THEN ! 17 hundred , 6 0 0, 6 9 0
		BEGIN
			IF N.SLOT.SECOND = 100 THEN ! 17 hundred
				BEGIN
					N.SLOT.RESULT := N.SLOT.FIRST ;
					S.SLOT.RESULT ~= N.SLOT.RESULT ;
					S.SLOT.RESULT := S.SLOT.RESULT + "00" ;
				END
			ELSE 
				BEGIN
					IF N.SLOT.SECOND < 10 THEN
						BEGIN
							IF N.SLOT.THIRD = 0 THEN
								BEGIN
									N.SLOT.RESULT := N.SLOT.FIRST * 100 + N.SLOT.SECOND ; ! 17 1 -> 1701
									S.SLOT.RESULT ~= N.SLOT.RESULT ;
								END
							ELSE
								BEGIN
									N.SLOT.RESULT := N.SLOT.FIRST * 100 + N.SLOT.SECOND * 10 + N.SLOT.THIRD ; ! 6 0 0, 6 9 0
									S.SLOT.RESULT ~= N.SLOT.RESULT ;
								END
						END
					ELSE
						BEGIN
							N.SLOT.RESULT := N.SLOT.FIRST * 100 + N.SLOT.SECOND ; ! 6 10
							S.SLOT.RESULT ~= N.SLOT.RESULT ;
						END
				END
		END
	ELSE ! 1000 700 50
		BEGIN
			IF N.SLOT.THIRD = 0 THEN
				BEGIN
					N.SLOT.RESULT := N.SLOT.FIRST + N.SLOT.SECOND ;
					S.SLOT.RESULT ~= N.SLOT.RESULT ;
				END
			ELSE
				BEGIN
					N.SLOT.RESULT := N.SLOT.FIRST + N.SLOT.SECOND + N.SLOT.THIRD ;
					S.SLOT.RESULT ~= N.SLOT.RESULT ;
				END
		END
		
	S.SLOT.ID := S.SLOT.RESULT + "00" ;
EOP

